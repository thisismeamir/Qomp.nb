Cell["\<\
The IEEE754 32-bit single precision floating-point number is so-called \
because of the 32-bits required to implement this standard. The bits are \
assigned as the figure above. 1 bit for the sign, 8 bits for the exponent, \
and 23 bits for the mantissa. 

To jump into code right away. I have implemented a function that given a \
number transforms it into IEEE754 32-bit single precision representation:\
\>", "Text",
 CellChangeTimes->{
  3.94963412395092*^9},ExpressionUUID->"0466049d-b8d1-491b-b1d5-52178e29b2e9"],

Cell[BoxData[
 RowBox[{
  RowBox[{"IEEE754SinglePrecision", "[", "number_Real", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "signBit", ",", "absNum", ",", "exponent", ",", "mantissa", ",", 
      "exponentBits", ",", "mantissaBits", ",", "intPart", ",", "fracPart", 
      ",", "normalizedMantissa", ",", "count"}], "}"}], ",", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
      RowBox[{"Step", " ", "1"}], ":", 
      RowBox[{"Compute", " ", "the", " ", "sign", " ", "bit"}]}], "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"signBit", "=", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"number", "<", "0"}], ",", "\"\<1\>\"", ",", "\"\<0\>\""}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"absNum", "=", 
      RowBox[{"Abs", "[", "number", "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"Special", " ", "Cases"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"Zero", " ", "case"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"absNum", "==", "0"}], ",", 
       RowBox[{
       "Return", "[", "\"\<00000000000000000000000000000000\>\"", "]"}]}], 
      "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", "Infinity", "*)"}], 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"absNum", "==", "\[Infinity]"}], ",", 
       RowBox[{"Return", "[", 
        RowBox[{"signBit", "<>", "\"\<11111111000000000000000000000000\>\""}],
         "]"}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", "NaN", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"!", 
        RowBox[{"NumericQ", "[", "absNum", "]"}]}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"Return", "[", 
        RowBox[{"signBit", "<>", "\"\<11111111111111111111111111111111\>\""}],
         "]"}]}], "]"}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Step", " ", "2"}], ":", 
       RowBox[{
       "Convert", " ", "number", " ", "to", " ", "binary", " ", "scientific", 
        " ", "notation"}]}], "*)"}], 
     RowBox[{"exponent", "=", 
      RowBox[{"Floor", "[", 
       RowBox[{"Log2", "[", "absNum", "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"Get", " ", "exponent"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"normalizedMantissa", "=", 
      RowBox[{"absNum", "/", 
       RowBox[{"2", "^", "exponent"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"Normalize", " ", "to", " ", "1.", "xxx", " ", "form"}], "*)"}],
      "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Step", " ", "3"}], ":", 
       RowBox[{"Compute", " ", "exponent", " ", "bits", " ", 
        RowBox[{"(", 
         RowBox[{"bias", " ", "127"}], ")"}]}]}], "*)"}], 
     RowBox[{"exponentBits", "=", 
      RowBox[{"IntegerString", "[", 
       RowBox[{
        RowBox[{"exponent", "+", "127"}], ",", "2", ",", "8"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Step", " ", "4"}], ":", 
       RowBox[{"Extract", " ", "mantissa", " ", "bits", " ", 
        RowBox[{"(", 
         RowBox[{"23", " ", "bits"}], ")"}]}]}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"normalizedMantissa", "-=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Remove", " ", "the", " ", "leading", " ", "1", " ", "in", " ", "1.", 
       "xxx"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"mantissaBits", "=", "\"\<\>\""}], ";", "\[IndentingNewLine]", 
     RowBox[{"fracPart", "=", "normalizedMantissa"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"count", "=", "0"}], ";", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"fracPart", ">", "0"}], "&&", 
        RowBox[{"count", "<", "23"}]}], ",", 
       RowBox[{
        RowBox[{"fracPart", "*=", "2"}], ";", "\[IndentingNewLine]", 
        RowBox[{"mantissaBits", "=", 
         RowBox[{"mantissaBits", "<>", 
          RowBox[{"ToString", "[", 
           RowBox[{"Floor", "[", "fracPart", "]"}], "]"}]}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"fracPart", "-=", 
         RowBox[{"Floor", "[", "fracPart", "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"count", "++"}], ";"}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Ensure", " ", "mantissa", " ", "is", " ", "exactly", " ", "23", " ", 
       "bits", " ", "long"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"mantissaBits", "=", 
      RowBox[{"StringPadRight", "[", 
       RowBox[{"mantissaBits", ",", "23", ",", "\"\<0\>\""}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Step", " ", "5"}], ":", 
       RowBox[{
       "Construct", " ", "the", " ", "final", " ", "IEEE754", " ", 
        "representation"}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"signBit", "<>", "exponentBits", "<>", "mantissaBits"}]}]}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.949578522143598*^9, 3.949578622584528*^9}, 
   3.949578982406711*^9},
 CellLabel->"In[9]:=",ExpressionUUID->"ea9a8a47-ee7c-4b36-897f-057e55410ae8"],

