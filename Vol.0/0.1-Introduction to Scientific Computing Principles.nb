98ac37f7710e"],

Cell[BoxData[
 RowBox[{
  RowBox[{"ToDecimal", "[", "binary_String", "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", "\n", "  ", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "digits", ",", " ", "values", ",", " ", "lengthOfIntegers", ",", " ", 
      "lengthOfFractions", ",", " ", "decimalValue"}], "}"}], ",", "\n", "\n",
     "  ", 
    RowBox[{"(*", " ", 
     RowBox[{
     "Split", " ", "the", " ", "string", " ", "at", " ", "the", " ", 
      "decimal", " ", "point"}], " ", "*)"}], "\n", "  ", 
    RowBox[{
     RowBox[{"digits", " ", "=", " ", 
      RowBox[{"StringSplit", "[", 
       RowBox[{"binary", ",", " ", "\"\<.\>\""}], "]"}]}], ";", "\n", "\n", 
     "  ", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Ensure", " ", "there", " ", "are", " ", "at", " ", "most", " ", "one", 
       " ", "or", " ", "zero", " ", "decimal", " ", "points"}], " ", "*)"}], 
     "\n", "  ", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "[", "digits", "]"}], " ", ">", " ", "2"}], ",", 
       " ", 
       RowBox[{"Return", "[", "$Failed", "]"}]}], "]"}], ";", "\n", "\n", 
     "  ", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Convert", " ", "characters", " ", "to", " ", "numbers", " ", "and", 
       " ", "validate"}], " ", "*)"}], "\n", "  ", 
     RowBox[{"values", " ", "=", " ", 
      RowBox[{"ToExpression", " ", "/@", " ", 
       RowBox[{"(", 
        RowBox[{"Characters", " ", "/@", " ", "digits"}], ")"}]}]}], ";", 
     "\n", "  ", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"!", 
        RowBox[{"AllTrue", "[", 
         RowBox[{
          RowBox[{"Flatten", "[", "values", "]"}], ",", " ", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"#", " ", "===", " ", "0"}], " ", "||", " ", 
             RowBox[{"#", " ", "===", " ", "1"}]}], ")"}], " ", "&"}]}], 
         "]"}]}], ",", " ", 
       RowBox[{"Return", "[", "$Failed", "]"}]}], "]"}], ";", "\n", "\n", 
     "  ", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Compute", " ", "decimal", " ", "value", " ", "for", " ", "integer", 
       " ", "part"}], " ", "*)"}], "\n", "  ", 
     RowBox[{"lengthOfIntegers", " ", "=", " ", 
      RowBox[{"Length", "[", 
       RowBox[{"values", "[", 
        RowBox[{"[", "1", "]"}], "]"}], "]"}]}], ";", "\n", "  ", 
     RowBox[{"decimalValue", " ", "=", " ", 
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"values", "[", 
           RowBox[{"[", "1", "]"}], "]"}], "[", 
          RowBox[{"[", "i", "]"}], "]"}], " ", "*", " ", 
         RowBox[{"2", "^", 
          RowBox[{"(", 
           RowBox[{"lengthOfIntegers", " ", "-", " ", "i"}], ")"}]}]}], ",", 
        " ", 
        RowBox[{"{", 
         RowBox[{"i", ",", " ", "1", ",", " ", "lengthOfIntegers"}], "}"}]}], 
       "]"}]}], ";", "\n", "\n", "  ", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Compute", " ", "fractional", " ", "part", " ", "if", " ", "it", " ", 
       "exists"}], " ", "*)"}], "\n", "  ", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "[", "digits", "]"}], " ", "==", " ", "2"}], ",", 
       "\n", "    ", 
       RowBox[{
        RowBox[{"lengthOfFractions", " ", "=", " ", 
         RowBox[{"Length", "[", 
          RowBox[{"values", "[", 
           RowBox[{"[", "2", "]"}], "]"}], "]"}]}], ";", "\n", "    ", 
        RowBox[{"decimalValue", " ", "+=", " ", 
         RowBox[{"Sum", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"values", "[", 
              RowBox[{"[", "2", "]"}], "]"}], "[", 
             RowBox[{"[", "i", "]"}], "]"}], " ", "*", " ", 
            RowBox[{"2", "^", 
             RowBox[{"-", "i"}]}]}], ",", " ", 
           RowBox[{"{", 
            RowBox[{"i", ",", " ", "1", ",", " ", "lengthOfFractions"}], 
            "}"}]}], "]"}]}]}]}], "\n", "  ", "]"}], ";", "\n", "\n", "  ", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Return", " ", "the", " ", "final", " ", "computed", " ", "decimal", 
       " ", "value"}], " ", "*)"}], "\n", "  ", "decimalValue"}]}], "\n", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.949575071851927*^9, 3.949575081425832*^9}},
 CellLabel->"In[1]:=",ExpressionUUID->"535d9d81-66ba-45ee-80ef-3db310c15258"],

