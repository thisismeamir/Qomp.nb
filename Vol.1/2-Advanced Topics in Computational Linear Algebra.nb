963b532ca98a"],

Cell[TextData[{
 StyleBox["Rank determination",
  FontWeight->"Bold"],
 ": the rank of ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       StyleBox["A", "TI"], TraditionalForm], "errors" -> {}, "input" -> "A", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "82029cd6-acf8-4fc7-8c89-9ab45057f56e"],
 " equals the rank of ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       SuperscriptBox[
        StyleBox["A", "TI"], "+"], TraditionalForm], "errors" -> {}, "input" -> 
     "A^+", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "a1bdbbb8-869b-4515-ad9a-6e7e073de2ed"],
 "."
}], "Item",
 CellChangeTimes->{{3.9547225644921722`*^9, 3.9547225700956697`*^9}, {
  3.954723058848873*^9, 3.954723074043437*^9}, {3.954723611466197*^9, 
