Cell["\<\
Many large - scale optimization problems involve sparse constraint matrices .\
\
\>", "Text",
 CellChangeTimes->{{3.954814801372381*^9, 
  3.954814802186969*^9}},ExpressionUUID->"ba2d897c-12ab-49c1-be83-\
6eaea35df02c"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "Simulate", " ", "a", " ", "large", " ", "linear", " ", "programming", " ",
     "problem"}], " ", "*)"}], "\n", 
  RowBox[{
   RowBox[{
    RowBox[{"nVariables", " ", "=", " ", "500"}], ";"}], "\n", 
   RowBox[{
    RowBox[{"nConstraints", " ", "=", " ", "300"}], ";"}], "\n", 
   RowBox[{
    RowBox[{"constraintsPerVariable", " ", "=", " ", "50"}], ";"}], " ", 
   RowBox[{"(*", " ", 
    RowBox[{"Each", " ", "variable", " ", "appears", " ", 
     RowBox[{"in", " ", "~", "5"}], " ", "constraints"}], " ", "*)"}], "\n", 
   "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Generate", " ", "random", " ", "sparse", " ", "constraint", " ", 
     "matrix"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"constraintMatrix", " ", "=", " ", 
     RowBox[{"SparseArray", "[", 
      RowBox[{
       RowBox[{"{", "}"}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"nConstraints", ",", " ", "nVariables"}], "}"}]}], "]"}]}], 
    ";"}], "\n", "\n", 
   RowBox[{"For", "[", 
    RowBox[{
     RowBox[{"var", " ", "=", " ", "1"}], ",", " ", 
     RowBox[{"var", " ", "<=", " ", "nVariables"}], ",", " ", 
     RowBox[{"var", "++"}], ",", "\n", "  ", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Select", " ", "random", " ", "constraints", " ", "that", " ", 
       "involve", " ", "this", " ", "variable"}], " ", "*)"}], "\n", "  ", 
     RowBox[{
      RowBox[{"varConstraints", " ", "=", " ", 
       RowBox[{"RandomSample", "[", 
        RowBox[{
         RowBox[{"Range", "[", "nConstraints", "]"}], ",", " ", 
         RowBox[{"RandomInteger", "[", 
          RowBox[{"{", 
           RowBox[{"1", ",", " ", 
            RowBox[{"constraintsPerVariable", " ", "*", " ", "2"}]}], "}"}], 
          "]"}]}], "]"}]}], ";", "\n", "  ", "\n", "  ", 
      RowBox[{"(*", " ", 
       RowBox[{"Assign", " ", "random", " ", "coefficients"}], " ", "*)"}], 
      "\n", "  ", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", " ", "=", " ", "1"}], ",", " ", 
        RowBox[{"i", " ", "<=", " ", 
         RowBox[{"Length", "[", "varConstraints", "]"}]}], ",", " ", 
        RowBox[{"i", "++"}], ",", "\n", "    ", 
        RowBox[{
         RowBox[{"constraint", " ", "=", " ", 
          RowBox[{"varConstraints", "[", 
           RowBox[{"[", "i", "]"}], "]"}]}], ";", "\n", "    ", 
         RowBox[{"coef", " ", "=", " ", 
          RowBox[{"RandomReal", "[", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"-", "10"}], ",", " ", "10"}], "}"}], "]"}]}], ";", "\n",
          "    ", 
         RowBox[{
          RowBox[{"constraintMatrix", "[", 
           RowBox[{"[", 
            RowBox[{"constraint", ",", " ", "var"}], "]"}], "]"}], " ", "=", 
          " ", "coef"}], ";"}]}], "\n", "  ", "]"}]}]}], "\n", "]"}], "\n", 
   "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Create", " ", "objective", " ", "function"}], " ", "*)"}], "\n", 
   
   RowBox[{
    RowBox[{"objectiveVector", " ", "=", " ", 
     RowBox[{"RandomReal", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"-", "1"}], ",", " ", "1"}], "}"}], ",", " ", "nVariables"}],
       "]"}]}], ";"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"Create", " ", "right"}], "-", 
     RowBox[{"hand", " ", "side"}]}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"rhs", " ", "=", " ", 
     RowBox[{"RandomReal", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", " ", "100"}], "}"}], ",", " ", "nConstraints"}], 
      "]"}]}], ";"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Analyze", " ", "constraint", " ", "matrix", " ", "sparsity"}], 
    " ", "*)"}], "\n", 
   RowBox[{
   "Print", "[", "\"\<Linear Programming Constraint Matrix:\>\"", "]"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<Dimensions: \>\"", ",", " ", 
     RowBox[{"Dimensions", "[", "constraintMatrix", "]"}]}], "]"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<Non-zero entries: \>\"", ",", " ", 
     RowBox[{"Length", "[", 
      RowBox[{"constraintMatrix", "[", "\"\<NonzeroValues\>\"", "]"}], 
      "]"}]}], "]"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<Density: \>\"", ",", " ", 
     RowBox[{
      RowBox[{
       RowBox[{"Length", "[", 
        RowBox[{"constraintMatrix", "[", "\"\<NonzeroValues\>\"", "]"}], 
        "]"}], "/", 
       RowBox[{"(", 
        RowBox[{"nConstraints", "*", "nVariables"}], ")"}]}], " ", "*", " ", 
      "100"}], ",", " ", "\"\<%\>\""}], "]"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Visualize", " ", "the", " ", "constraint", " ", "matrix", " ", 
     "pattern"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"subsetConstraintMatrix", " ", "=", " ", 
     RowBox[{"constraintMatrix", "[", 
      RowBox[{"[", 
       RowBox[{
        RowBox[{"1", " ", ";;", " ", "50"}], ",", " ", 
        RowBox[{"1", " ", ";;", " ", "50"}]}], "]"}], "]"}]}], ";"}], "\n", 
   "\n", 
   RowBox[{"MatrixPlot", "[", 
    RowBox[{"subsetConstraintMatrix", ",", " ", "\n", "  ", 
     RowBox[{
     "PlotLabel", " ", "->", " ", 
      "\"\<LP Constraint Matrix Pattern (Subset)\>\""}], ",", "\n", "  ", 
     RowBox[{"ColorFunction", " ", "->", " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"#", " ", "==", " ", "0"}], ",", " ", "White", ",", " ", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"#", " ", ">", " ", "0"}], ",", " ", "Blue", ",", " ", 
            "Red"}], "]"}]}], "]"}], " ", "&"}], ")"}]}], ",", "\n", "  ", 
     RowBox[{"FrameLabel", " ", "->", " ", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"\"\<Constraints\>\"", ",", " ", "None"}], "}"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"\"\<Variables\>\"", ",", " ", "None"}], "}"}]}], "}"}]}]}], 
    "]"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Simulate", " ", "solving", " ", "the", " ", "LP"}], " ", "*)"}], 
   "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Note", ":", " ", 
     RowBox[{
     "Actually", " ", "solving", " ", "this", " ", "would", " ", "require", 
      " ", "LinearProgramming", " ", "function"}]}], " ", "*)"}], "\n", 
   RowBox[{
   "Print", "[", 
    "\"\<In a real LP solver, sparse algorithms would exploit this structure \
for:\>\"", "]"}], "\n", 
   RowBox[{
   "Print", "[", "\"\<1. Factorization of the basis matrix\>\"", "]"}], "\n", 
   
   RowBox[{
   "Print", "[", "\"\<2. Updating the factorization during pivoting\>\"", 
    "]"}], "\n", 
   RowBox[{
   "Print", "[", "\"\<3. Computing search directions\>\"", "]"}]}]}]], "Input",\

 CellChangeTimes->{{3.954814809621654*^9, 3.9548148227262383`*^9}},
