Cell["\<\
In NLP, term - document matrices are extremely sparse because most words \
appear in only a small fraction of documents .\
\>", "Text",
 CellChangeTimes->{{3.954814676005232*^9, 
  3.954814677083363*^9}},ExpressionUUID->"3fed4c90-41b7-4438-83a7-\
b24194668a0f"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"Simulate", " ", "a", " ", "term"}], "-", 
    RowBox[{
    "document", " ", "matrix", " ", "with", " ", "100", " ", "documents", " ",
      "and", " ", "1000", " ", "terms"}]}], " ", "*)"}], "\n", 
  RowBox[{
   RowBox[{
    RowBox[{"nDocs", " ", "=", " ", "100"}], ";"}], "\n", 
   RowBox[{
    RowBox[{"nTerms", " ", "=", " ", "1000"}], ";"}], "\n", 
   RowBox[{
    RowBox[{"averageTermsPerDoc", " ", "=", " ", "50"}], ";"}], " ", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Each", " ", "document", " ", "uses", " ", "about", " ", "50", " ", 
     "unique", " ", "terms"}], " ", "*)"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"Generate", " ", "sparse", " ", "term"}], "-", 
     RowBox[{"document", " ", "matrix"}]}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"termDocMatrix", " ", "=", " ", 
     RowBox[{"SparseArray", "[", 
      RowBox[{
       RowBox[{"{", "}"}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"nTerms", ",", " ", "nDocs"}], "}"}]}], "]"}]}], ";"}], "\n", 
   "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"Fill", " ", "with", " ", "random", " ", "data"}], " ", "-", " ", 
     RowBox[{
     "each", " ", "document", " ", "has", " ", "a", " ", "small", " ", 
      "subset", " ", "of", " ", "terms"}]}], " ", "*)"}], "\n", 
   RowBox[{"For", "[", 
    RowBox[{
     RowBox[{"doc", " ", "=", " ", "1"}], ",", " ", 
     RowBox[{"doc", " ", "<=", " ", "nDocs"}], ",", " ", 
     RowBox[{"doc", "++"}], ",", "\n", "  ", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Select", " ", "random", " ", "terms", " ", "for", " ", "this", " ", 
       "document"}], " ", "*)"}], "\n", "  ", 
     RowBox[{
      RowBox[{"docTerms", " ", "=", " ", 
       RowBox[{"RandomSample", "[", 
        RowBox[{
         RowBox[{"Range", "[", "nTerms", "]"}], ",", " ", 
         RowBox[{"RandomInteger", "[", 
          RowBox[{"{", 
           RowBox[{"30", ",", " ", 
            RowBox[{"averageTermsPerDoc", " ", "*", " ", "2"}]}], "}"}], 
          "]"}]}], "]"}]}], ";", "\n", "  ", "\n", "  ", 
      RowBox[{"(*", " ", 
       RowBox[{"Assign", " ", "random", " ", "term", " ", "frequencies"}], 
       " ", "*)"}], "\n", "  ", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", " ", "=", " ", "1"}], ",", " ", 
        RowBox[{"i", " ", "<=", " ", 
         RowBox[{"Length", "[", "docTerms", "]"}]}], ",", " ", 
        RowBox[{"i", "++"}], ",", "\n", "    ", 
        RowBox[{
         RowBox[{"term", " ", "=", " ", 
          RowBox[{"docTerms", "[", 
           RowBox[{"[", "i", "]"}], "]"}]}], ";", "\n", "    ", 
         RowBox[{"(*", " ", 
          RowBox[{
           RowBox[{"Term", " ", "frequency", " ", "follows", " ", 
            RowBox[{"Zipf", "'"}], "s", " ", "law"}], " ", "-", " ", 
           RowBox[{
           "a", " ", "few", " ", "terms", " ", "appear", " ", 
            "frequently"}]}], " ", "*)"}], "\n", "    ", 
         RowBox[{"frequency", " ", "=", " ", 
          RowBox[{"RandomInteger", "[", 
           RowBox[{"{", 
            RowBox[{"1", ",", " ", "10"}], "}"}], "]"}]}], ";", "\n", "    ", 
         
         RowBox[{
          RowBox[{"termDocMatrix", "[", 
           RowBox[{"[", 
            RowBox[{"term", ",", " ", "doc"}], "]"}], "]"}], " ", "=", " ", 
          "frequency"}], ";"}]}], "\n", "  ", "]"}]}]}], "\n", "]"}], "\n", 
   "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Analyze", " ", "sparsity"}], " ", "*)"}], "\n", 
   RowBox[{"Print", "[", "\"\<Term-Document Matrix:\>\"", "]"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<Dimensions: \>\"", ",", " ", 
     RowBox[{"Dimensions", "[", "termDocMatrix", "]"}]}], "]"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<Non-zero entries: \>\"", ",", " ", 
     RowBox[{"Length", "[", 
      RowBox[{"termDocMatrix", "[", "\"\<NonzeroValues\>\"", "]"}], "]"}]}], 
    "]"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<Density: \>\"", ",", " ", 
     RowBox[{
      RowBox[{
       RowBox[{"Length", "[", 
        RowBox[{"termDocMatrix", "[", "\"\<NonzeroValues\>\"", "]"}], "]"}], 
       "/", 
       RowBox[{"(", 
        RowBox[{"nTerms", "*", "nDocs"}], ")"}]}], " ", "*", " ", "100"}], 
     ",", " ", "\"\<%\>\""}], "]"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"Visualize", " ", "the", " ", "term"}], "-", 
     RowBox[{"document", " ", "matrix"}]}], " ", "*)"}], "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"We", "'"}], "ll", " ", "visualize", " ", "a", " ", "smaller", 
     " ", "subset", " ", "for", " ", "clarity"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"subsetSize", " ", "=", " ", "50"}], ";"}], "\n", 
   RowBox[{
    RowBox[{"subsetTermDocMatrix", " ", "=", " ", 
     RowBox[{"termDocMatrix", "[", 
      RowBox[{"[", 
       RowBox[{
        RowBox[{"1", " ", ";;", " ", "subsetSize"}], ",", " ", 
        RowBox[{"1", " ", ";;", " ", "subsetSize"}]}], "]"}], "]"}]}], ";"}], 
   "\n", "\n", 
   RowBox[{"MatrixPlot", "[", 
    RowBox[{"subsetTermDocMatrix", ",", " ", "\n", "  ", 
     RowBox[{
     "PlotLabel", " ", "->", " ", "\"\<Term-Document Matrix (Subset)\>\""}], 
     ",", "\n", "  ", 
     RowBox[{"ColorFunction", " ", "->", " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"#", " ", "==", " ", "0"}], ",", " ", "White", ",", " ", 
          RowBox[{
           RowBox[{"ColorData", "[", "\"\<Rainbow\>\"", "]"}], "[", "#", 
           "]"}]}], "]"}], " ", "&"}], ")"}]}], ",", "\n", "  ", 
     RowBox[{"FrameLabel", " ", "->", " ", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"\"\<Terms\>\"", ",", " ", "None"}], "}"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"\"\<Documents\>\"", ",", " ", "None"}], "}"}]}], "}"}]}]}], 
    "]"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"Compute", " ", "Term", " ", "Frequency"}], "-", 
     RowBox[{"Inverse", " ", "Document", " ", "Frequency", " ", 
      RowBox[{"(", 
       RowBox[{"TF", "-", "IDF"}], ")"}]}]}], " ", "*)"}], "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "A", " ", "common", " ", "transformation", " ", "in", " ", "NLP", " ", 
     "that", " ", "benefits", " ", "from", " ", "sparse", " ", 
     "representation"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"docFrequencies", " ", "=", " ", 
     RowBox[{"Table", "[", "\n", "  ", 
      RowBox[{
       RowBox[{"Count", "[", 
        RowBox[{
         RowBox[{"termDocMatrix", "[", 
          RowBox[{"[", 
           RowBox[{"term", ",", " ", "All"}], "]"}], "]"}], ",", " ", 
         RowBox[{"x_", " ", "/;", " ", 
          RowBox[{"x", " ", ">", " ", "0"}]}]}], "]"}], ",", "\n", "  ", 
       RowBox[{"{", 
        RowBox[{"term", ",", " ", "1", ",", " ", "nTerms"}], "}"}]}], "\n", 
      "]"}]}], ";"}], "\n", "\n", 
   RowBox[{
    RowBox[{"idf", " ", "=", " ", 
     RowBox[{"Log", "[", 
      RowBox[{"nDocs", "/", 
       RowBox[{"(", 
        RowBox[{"docFrequencies", " ", "+", "0.01"}], ")"}]}], "]"}]}], ";"}],
    "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"Apply", " ", "TF"}], "-", 
     RowBox[{"IDF", " ", "transformation"}]}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"tfidfMatrix", " ", "=", " ", 
     RowBox[{"SparseArray", "[", 
      RowBox[{
       RowBox[{"{", "}"}], ",", " ", 
       RowBox[{"Dimensions", "[", "termDocMatrix", "]"}]}], "]"}]}], ";"}], 
   "\n", 
   RowBox[{
    RowBox[{"nonZeros", " ", "=", " ", 
     RowBox[{"termDocMatrix", "[", "\"\<NonzeroPositions\>\"", "]"}]}], ";"}],
    "\n", 
   RowBox[{
    RowBox[{"values", " ", "=", " ", 
     RowBox[{"termDocMatrix", "[", "\"\<NonzeroValues\>\"", "]"}]}], ";"}], 
   "\n", "\n", 
   RowBox[{"For", "[", 
    RowBox[{
     RowBox[{"i", " ", "=", " ", "1"}], ",", " ", 
     RowBox[{"i", " ", "<=", " ", 
      RowBox[{"Length", "[", "nonZeros", "]"}]}], ",", " ", 
     RowBox[{"i", "++"}], ",", "\n", "  ", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{"term", ",", " ", "doc"}], "}"}], " ", "=", " ", 
       RowBox[{"nonZeros", "[", 
        RowBox[{"[", "i", "]"}], "]"}]}], ";", "\n", "  ", 
      RowBox[{"tf", " ", "=", " ", 
       RowBox[{"values", "[", 
        RowBox[{"[", "i", "]"}], "]"}]}], ";", "\n", "  ", 
      RowBox[{
       RowBox[{"tfidfMatrix", "[", 
        RowBox[{"[", 
         RowBox[{"term", ",", " ", "doc"}], "]"}], "]"}], " ", "=", " ", 
       RowBox[{"tf", " ", "*", " ", 
        RowBox[{"idf", "[", 
         RowBox[{"[", "term", "]"}], "]"}]}]}], ";"}]}], "\n", "]"}], "\n", 
   "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"Visualize", " ", "the", " ", "TF"}], "-", 
     RowBox[{"IDF", " ", "weights"}]}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"subsetTfidfMatrix", " ", "=", " ", 
     RowBox[{"tfidfMatrix", "[", 
      RowBox[{"[", 
       RowBox[{
        RowBox[{"1", " ", ";;", " ", "subsetSize"}], ",", " ", 
        RowBox[{"1", " ", ";;", " ", "subsetSize"}]}], "]"}], "]"}]}], ";"}], 
   "\n", "\n", 
   RowBox[{"MatrixPlot", "[", 
    RowBox[{"subsetTfidfMatrix", ",", " ", "\n", "  ", 
     RowBox[{"PlotLabel", " ", "->", " ", "\"\<TF-IDF Matrix (Subset)\>\""}], 
     ",", "\n", "  ", 
     RowBox[{"ColorFunction", " ", "->", " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"#", " ", "==", " ", "0"}], ",", " ", "White", ",", " ", 
          RowBox[{
           RowBox[{"ColorData", "[", "\"\<Rainbow\>\"", "]"}], "[", "#", 
           "]"}]}], "]"}], " ", "&"}], ")"}]}], ",", "\n", "  ", 
     RowBox[{"FrameLabel", " ", "->", " ", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"\"\<Terms\>\"", ",", " ", "None"}], "}"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"\"\<Documents\>\"", ",", " ", "None"}], "}"}]}], "}"}]}]}], 
    "]"}]}]}]], "Input",
