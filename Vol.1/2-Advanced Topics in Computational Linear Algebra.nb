Cell["Matrix Exponential", "Section",
 CellChangeTimes->{{3.9508593814726057`*^9, 
  3.950859384416512*^9}},ExpressionUUID->"4dc1b099-9f13-4a8e-b90c-\
7d41e99ec2a5"],

Cell[TextData[{
 "The matrix exponential is one of the most important matrix functions, with \
applications in solving systems of differential equations, continuous-time \
Markov chains, and Lie group theory. For a square matrix A, the matrix \
exponential is defined using the power series:\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SuperscriptBox[
          StyleBox["e", "TI"], 
          StyleBox["A", "TI"]], "\[LongEqual]", 
         UnderoverscriptBox["\[Sum]", 
          RowBox[{
            StyleBox["k", "TI"], "\[LongEqual]", "0"}], "\[Infinity]", 
          LimitsPositioning -> True], 
         FractionBox[
          SuperscriptBox[
           StyleBox["A", "TI"], 
           StyleBox["k", "TI"]], 
          RowBox[{
            StyleBox["k", "TI"], "!"}]], "\[LongEqual]", 
         StyleBox["I", "TI"], "+", 
         StyleBox["A", "TI"], "+", 
         FractionBox[
          SuperscriptBox[
           StyleBox["A", "TI"], "2"], "2!"], "+", 
         FractionBox[
          SuperscriptBox[
           StyleBox["A", "TI"], "3"], "3!"], "+", "\[Ellipsis]"}], 
       TraditionalForm], "errors" -> {}, "input" -> 
     "e^A = \\sum_{k=0}^{\\infty} \\frac{A^k}{k!} = I + A + \\frac{A^2}{2!} + \
\\frac{A^3}{3!} + \\ldots", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "6915af52-9b7b-485b-9aee-c2a75d54184a"],
 "\nKey properties of the matrix exponential include:"
}], "Text",
 CellChangeTimes->{{3.954742465508553*^9, 
  3.954742478084485*^9}},ExpressionUUID->"5ac65c46-cca2-4d5c-9bad-\
ed1624aaf842"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       SuperscriptBox[
        StyleBox["e", "TI"], 
        RowBox[{
          StyleBox["A", "TI"], "+", 
          StyleBox["B", "TI"]}]], TraditionalForm], "errors" -> {}, "input" -> 
     "e^{A+B}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "6e0b1fd1-c6f6-4653-80e9-1dbe23645434"],
 " = ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SuperscriptBox[
          StyleBox["e", "TI"], 
          StyleBox["A", "TI"]], 
         SuperscriptBox[
          StyleBox["e", "TI"], 
          StyleBox["B", "TI"]]}], TraditionalForm], "errors" -> {}, "input" -> 
     "e^A e^B", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "f5b8a4fa-bb94-4653-98e1-0fc4ad4839f1"],
 " if and only if ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["A", "TI"], 
         StyleBox["B", "TI"], "\[LongEqual]", 
         StyleBox["B", "TI"], 
         StyleBox["A", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "AB = BA", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "0521521f-3fd4-4d10-9a14-09eadb1eb2c7"],
 " (matrices commute)"
}], "Item",
 CellChangeTimes->{{3.954742499377164*^9, 
  3.954742535674162*^9}},ExpressionUUID->"d9698fc3-8034-45cd-b9b8-\
d2fd0972d4e4"],

Cell[TextData[Cell[BoxData[
 FormBox[
  TemplateBox[<|"boxes" -> FormBox[
      RowBox[{
        SuperscriptBox[
         StyleBox["e", "TI"], 
         SuperscriptBox[
          StyleBox["A", "TI"], 
          StyleBox["T", "TI"]]], "\[LongEqual]", 
        SuperscriptBox[
         RowBox[{"(", 
           SuperscriptBox[
            StyleBox["e", "TI"], 
            StyleBox["A", "TI"]], ")"}], 
         StyleBox["T", "TI"]]}], TraditionalForm], "errors" -> {}, "input" -> 
    "e^{A^T} = (e^A)^T", "state" -> "Boxes"|>,
   "TeXAssistantTemplate"], 
  TraditionalForm]],ExpressionUUID->"b2dc9958-9572-4c20-bcba-a09bb606a9fa"]], \
"Item",
 CellChangeTimes->{{3.954742499377164*^9, 3.9547425060775137`*^9}, 
   3.954742540055258*^9},ExpressionUUID->"9ace8f0c-fca5-4f11-9fa2-\
ada19ca74d98"],

Cell[TextData[{
 "If ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       StyleBox["A", "TI"], TraditionalForm], "errors" -> {}, "input" -> "A", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "08671c3b-d319-47b9-a8ea-1d934af0f72c"],
 " is diagonalizable as A",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"\[LongEqual]", 
         StyleBox["P", "TI"], 
         StyleBox["D", "TI"], 
         SuperscriptBox[
          StyleBox["P", "TI"], "-1"]}], TraditionalForm], "errors" -> {}, 
     "input" -> " = PDP^{-1}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "6bad9a90-b9ad-4a1d-842e-ed1edb677e9c"],
 ", then ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SuperscriptBox[
          StyleBox["e", "TI"], 
          StyleBox["A", "TI"]], "\[LongEqual]", 
         StyleBox["P", "TI"], 
         SuperscriptBox[
          StyleBox["e", "TI"], 
          StyleBox["D", "TI"]], 
         SuperscriptBox[
          StyleBox["P", "TI"], "-1"]}], TraditionalForm], "errors" -> {}, 
     "input" -> "e^A = Pe^DP^{-1}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "1f7aa3e1-cbde-4e6b-b953-2cc320733bf2"],
 " where ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       SuperscriptBox[
        StyleBox["e", "TI"], 
        StyleBox["D", "TI"]], TraditionalForm], "errors" -> {}, "input" -> 
     "e^D", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "60c99335-cd96-42c7-a22e-813e550abb24"],
 " is a diagonal matrix"
}], "Item",
 CellChangeTimes->{{3.954742499377164*^9, 3.954742511333075*^9}, {
  3.9547425437591553`*^9, 
  3.954742553040062*^9}},ExpressionUUID->"6b426817-3fc6-45eb-a215-\
168d95cc53de"],

Cell[TextData[{
 "For any square matrix, ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"det", 
         RowBox[{"(", 
           SuperscriptBox[
            StyleBox["e", "TI"], 
            StyleBox["A", "TI"]], ")"}], "\[LongEqual]", 
         SuperscriptBox[
          StyleBox["e", "TI"], 
          RowBox[{"tr", "(", 
            StyleBox["A", "TI"], ")"}]]}], TraditionalForm], "errors" -> {}, 
     "input" -> "\\det(e^A) = e^{\\text{tr}(A)}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "8c46ff2c-6107-4a86-9c91-12dfafe90d22"]
}], "Item",
 CellChangeTimes->{{3.954742499377164*^9, 3.954742514980502*^9}, 
   3.954742559621369*^9},ExpressionUUID->"73141768-5e80-43de-9ebd-\
1216b57adf0a"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       SuperscriptBox[
        StyleBox["e", "TI"], 
        StyleBox["A", "TI"]], TraditionalForm], "errors" -> {}, "input" -> 
     "e^A", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "cd09a4d3-0c75-49de-8467-6b1959d2043a"],
 " is always invertible with ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
            SuperscriptBox[
             StyleBox["e", "TI"], 
             StyleBox["A", "TI"]], ")"}], "-1"], "\[LongEqual]", 
         SuperscriptBox[
          StyleBox["e", "TI"], 
          RowBox[{"-", 
            StyleBox["A", "TI"]}]]}], TraditionalForm], "errors" -> {}, 
     "input" -> "(e^A)^{-1} = e^{-A}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "19ef5981-2c0b-4afa-85ca-f44598f13f57"]
}], "Item",
 CellChangeTimes->{{3.954742499377164*^9, 3.9547425151994257`*^9}, {
  3.9547425627838*^9, 
  3.954742566494856*^9}},ExpressionUUID->"16e93223-edd4-41f5-a58e-\
8051c41f9531"],

Cell[TextData[{
 "The matrix exponential is especially important for solving systems of \
first-order linear differential equations:\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         FractionBox[
          RowBox[{
            StyleBox["d", "TI"], 
            OverscriptBox[
             StyleBox["x", "TI"], "\[RightVector]"]}], 
          RowBox[{
            StyleBox["d", "TI"], 
            StyleBox["t", "TI"]}]], "\[LongEqual]", 
         StyleBox["A", "TI"], 
         OverscriptBox[
          StyleBox["x", "TI"], "\[RightVector]"]}], TraditionalForm], 
     "errors" -> {}, "input" -> "\\frac{d\\vec{x}}{dt} = A\\vec{x}", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "906df67a-c52e-4505-93f7-6ef59aac900f"],
 "; whose solution is ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         OverscriptBox[
          StyleBox["x", "TI"], "\[RightVector]"], 
         RowBox[{"(", 
           StyleBox["t", "TI"], ")"}], "\[LongEqual]", 
         SuperscriptBox[
          StyleBox["e", "TI"], 
          RowBox[{
            StyleBox["A", "TI"], 
            StyleBox["t", "TI"]}]], 
         OverscriptBox[
          StyleBox["x", "TI"], "\[RightVector]"], 
         RowBox[{"(", "0", ")"}]}], TraditionalForm], "errors" -> {}, "input" -> 
     "\\vec{x}(t) = e^{At}\\vec{x}(0)", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "049e57b0-50e4-4f75-91d9-27b570b16acc"],
 "."
}], "Text",
 CellChangeTimes->{
  3.954742523199912*^9, {3.954742570795703*^9, 
   3.954742580060526*^9}},ExpressionUUID->"e9d5dc52-4701-4671-b30b-\
68716ae2f2bf"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "Create", " ", "a", " ", "simple", " ", "2", "\[Times]", "2", " ", 
    "matrix"}], " ", "*)"}], "\n", 
  RowBox[{
   RowBox[{
    RowBox[{"A", " ", "=", " ", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", " ", "1"}], "}"}], ",", " ", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"-", "1"}], ",", " ", "0"}], "}"}]}], "}"}]}], ";"}], "\n", 
   RowBox[{"Print", "[", "\"\<Matrix A:\>\"", "]"}], "\n", 
   RowBox[{"MatrixForm", "[", "A", "]"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Compute", " ", "the", " ", "matrix", " ", "exponential"}], " ", 
    "*)"}], "\n", 
   RowBox[{
    RowBox[{"expA", " ", "=", " ", 
     RowBox[{"MatrixExp", "[", "A", "]"}]}], ";"}], "\n", 
   RowBox[{"Print", "[", "\"\<Matrix exponential e^A:\>\"", "]"}], "\n", 
   RowBox[{"MatrixForm", "[", "expA", "]"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Compute", " ", "using", " ", "power", " ", "series", " ", 
     "approximation", " ", 
     RowBox[{"(", 
      RowBox[{"for", " ", "demonstration"}], ")"}]}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"powerSeriesExp", "[", 
      RowBox[{"mat_", ",", " ", "terms_"}], "]"}], " ", ":=", " ", "\n", "  ", 
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"MatrixPower", "[", 
         RowBox[{"mat", ",", " ", "k"}], "]"}], "/", 
        RowBox[{"Factorial", "[", "k", "]"}]}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"k", ",", " ", "0", ",", " ", "terms"}], "}"}]}], "]"}]}], 
    ";"}], "\n", "  ", "\n", 
   RowBox[{
    RowBox[{"expA_series", " ", "=", " ", 
     RowBox[{"powerSeriesExp", "[", 
      RowBox[{"A", ",", " ", "10"}], "]"}]}], ";"}], "\n", 
   RowBox[{"Print", "[", "\"\<e^A via power series (10 terms):\>\"", "]"}], 
   "\n", 
   RowBox[{"MatrixForm", "[", "expA_series", "]"}], "\n", 
   RowBox[{"Print", "[", "\"\<Difference with direct computation:\>\"", "]"}],
    "\n", 
   RowBox[{"MatrixForm", "[", 
    RowBox[{
     RowBox[{"expA", " ", "-", " ", "expA_series"}], " ", "//", " ", "Chop"}],
     "]"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"For", " ", "this", " ", "particular", " ", "matrix"}], ",", " ", 
     RowBox[{
      RowBox[{"e", "^", "A"}], " ", "represents", " ", "a", " ", 
      "rotation"}]}], " ", "*)"}], "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"Let", "'"}], "s", " ", "verify", " ", "by", " ", "applying", 
     " ", "it", " ", "to", " ", "some", " ", "vectors"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"v", " ", "=", " ", 
     RowBox[{"{", 
      RowBox[{"1", ",", " ", "0"}], "}"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"expA", ".", "v"}], " ", "//", " ", "N"}], "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"This", " ", "should", " ", "be", " ", "approximately", " ", 
      RowBox[{"{", 
       RowBox[{"0", ",", " ", "1"}], "}"}]}], ",", " ", 
     RowBox[{"a", " ", "90", "\[Degree]", " ", "rotation"}]}], " ", "*)"}], 
   "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Solving", " ", "a", " ", "differential", " ", "equation", " ", 
     "system"}], " ", "*)"}], "\n", 
   RowBox[{
   "Print", "[", "\"\<Solving differential equation x'(t) = Ax:\>\"", "]"}], 
   "\n", 
   RowBox[{"Print", "[", "\"\<Initial condition x(0) = {1, 0}\>\"", "]"}], 
   "\n", 
   RowBox[{"Print", "[", "\"\<Solution at t = \[Pi]/2:\>\"", "]"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"MatrixExp", "[", 
      RowBox[{"A", " ", "*", " ", 
       RowBox[{"(", 
        RowBox[{"Pi", "/", "2"}], ")"}]}], "]"}], ".", 
     RowBox[{"{", 
      RowBox[{"1", ",", " ", "0"}], "}"}]}], " ", "//", " ", "N"}], "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"Should", " ", "be", " ", "approximately", " ", 
      RowBox[{"{", 
       RowBox[{"0", ",", " ", "1"}], "}"}]}], ",", " ", 
     RowBox[{"a", " ", "90", "\[Degree]", " ", "rotation"}]}], " ", "*)"}], 
   "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Verify", " ", "properties"}], " ", "*)"}], "\n", 
   RowBox[{"Print", "[", "\"\<Properties of matrix exponential:\>\"", "]"}], 
   "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<det(e^A) = \>\"", ",", " ", 
     RowBox[{
      RowBox[{"Det", "[", "expA", "]"}], " ", "//", " ", "N"}]}], "]"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<e^tr(A) = \>\"", ",", " ", 
     RowBox[{
      RowBox[{"Exp", "[", 
       RowBox[{"Tr", "[", "A", "]"}], "]"}], " ", "//", " ", "N"}]}], "]"}], 
   "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<(e^A)^(-1) = e^(-A)? \>\"", ",", " ", "\n", "  ", 
     RowBox[{
      RowBox[{"Norm", "[", 
       RowBox[{
        RowBox[{"Inverse", "[", "expA", "]"}], " ", "-", " ", 
        RowBox[{"MatrixExp", "[", 
         RowBox[{"-", "A"}], "]"}]}], "]"}], " ", "<", " ", 
      RowBox[{"10", "^", 
       RowBox[{"-", "10"}]}]}]}], "]"}]}]}]], "Input",
