  3.954807772266267*^9},ExpressionUUID->"60e0ee02-1abe-469c-8c44-\
9650368c9816"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", "Example", " ", "*)"}], "\n", 
  RowBox[{
   RowBox[{
    RowBox[{"c", " ", "=", " ", 
     RowBox[{"{", 
      RowBox[{
      "1", ",", " ", "2", ",", " ", "3", ",", " ", "4", ",", " ", "5"}], 
      "}"}]}], ";"}], " ", 
   RowBox[{"(*", " ", 
    RowBox[{"First", " ", "column"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"r", " ", "=", " ", 
     RowBox[{"{", 
      RowBox[{
      "1", ",", " ", "6", ",", " ", "7", ",", " ", "8", ",", " ", "9"}], 
      "}"}]}], ";"}], " ", 
   RowBox[{"(*", " ", 
    RowBox[{"First", " ", "row"}], " ", "*)"}], "\n", "\n", 
   RowBox[{
    RowBox[{"T", " ", "=", " ", 
     RowBox[{"ToeplitzMatrix", "[", 
      RowBox[{"c", ",", " ", "r"}], "]"}]}], ";"}], "\n", 
   RowBox[{"Print", "[", "\"\<Toeplitz Matrix:\>\"", "]"}], "\n", 
   RowBox[{"MatrixForm", "[", "T", "]"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Verify", " ", "the", " ", "Toeplitz", " ", "property"}], " ", 
    "*)"}], "\n", 
   RowBox[{"Print", "[", "\"\<Checking Toeplitz property:\>\"", "]"}], "\n", 
   RowBox[{
    RowBox[{"allDiags", " ", "=", " ", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"Diagonal", "[", 
        RowBox[{"T", ",", " ", "k"}], "]"}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"k", ",", " ", 
         RowBox[{"-", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Length", "[", "c", "]"}], " ", "-", " ", "1"}], ")"}]}], 
         ",", " ", 
         RowBox[{
          RowBox[{"Length", "[", "r", "]"}], " ", "-", " ", "1"}]}], "}"}]}], 
      "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"allDiagsEqual", " ", "=", " ", 
     RowBox[{"And", " ", "@@", " ", 
      RowBox[{"Map", "[", "\n", "  ", 
       RowBox[{
        RowBox[{
         RowBox[{"Apply", "[", 
          RowBox[{"Equal", ",", " ", "#"}], "]"}], " ", "&"}], ",", "\n", 
        "  ", "allDiags"}], "\n", "]"}]}]}], ";"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{
    "\"\<All elements along each diagonal are equal? \>\"", ",", " ", 
     "allDiagsEqual"}], "]"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Efficient", " ", "storage", " ", "representation"}], " ", "*)"}],
    "\n", 
   RowBox[{
   "Print", "[", "\"\<Efficient representation (only 2n-1 values):\>\"", 
    "]"}], "\n", 
   RowBox[{
    RowBox[{"diagonalValues", " ", "=", " ", 
     RowBox[{"Join", "[", 
      RowBox[{
       RowBox[{"Reverse", "[", 
        RowBox[{"r", "[", 
         RowBox[{"[", 
          RowBox[{"2", " ", ";;", " ", "All"}], "]"}], "]"}], "]"}], ",", " ",
        "c"}], "]"}]}], ";"}], "\n", "diagonalValues", "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Solving", " ", "Toeplitz", " ", "systems"}], " ", "*)"}], "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Using", " ", "the", " ", "Levinson", " ", "algorithm", " ", 
     RowBox[{"(", 
      RowBox[{"built", " ", "into", " ", "Mathematica"}], ")"}]}], " ", 
    "*)"}], "\n", 
   RowBox[{
    RowBox[{"b", " ", "=", " ", 
     RowBox[{"{", 
      RowBox[{
      "10", ",", " ", "20", ",", " ", "30", ",", " ", "40", ",", " ", "50"}], 
      "}"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"solution", " ", "=", " ", 
     RowBox[{"LinearSolve", "[", 
      RowBox[{"T", ",", " ", "b"}], "]"}]}], ";"}], "\n", 
   RowBox[{"Print", "[", "\"\<Solution to Tx = b:\>\"", "]"}], "\n", 
   "solution", "\n", 
   RowBox[{"Print", "[", "\"\<Verification T.x = b:\>\"", "]"}], "\n", 
   RowBox[{"T", ".", "solution"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Defining", " ", "a", " ", "symmetric", " ", "Toeplitz", " ", "matrix"}], 
    " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"symC", " ", "=", " ", 
     RowBox[{"{", 
      RowBox[{
      "5", ",", " ", "4", ",", " ", "3", ",", " ", "2", ",", " ", "1"}], 
      "}"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"symR", " ", "=", " ", 
     RowBox[{"{", 
