          RowBox[{"2", " ", ";;", " ", "All"}], "]"}], "]"}], "]"}], ",", " ",
        "c"}], "]"}]}], ";"}], "\n", "diagonalValues", "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Solving", " ", "Toeplitz", " ", "systems"}], " ", "*)"}], "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Using", " ", "the", " ", "Levinson", " ", "algorithm", " ", 
