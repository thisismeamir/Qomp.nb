  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["A", "TI"], "\[LongEqual]", 
         StyleBox["Q", "TI"], 
         StyleBox["T", "TI"], 
         SuperscriptBox[
          StyleBox["Q", "TI"], "*"]}], TraditionalForm], "errors" -> {}, 
     "input" -> "A = QTQ^*", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "568c2f01-1ab8-4860-9668-7121fc7ae203"],
 ", then ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["f", "TI"], 
         RowBox[{"(", 
           StyleBox["A", "TI"], ")"}], "\[LongEqual]", 
         StyleBox["Q", "TI"], 
         StyleBox["f", "TI"], 
         RowBox[{"(", 
           StyleBox["T", "TI"], ")"}], 
         SuperscriptBox[
          StyleBox["Q", "TI"], "*"]}], TraditionalForm], "errors" -> {}, 
     "input" -> "f(A) = Qf(T)Q^*", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "f587a7ea-d237-4ed4-ae81-5cf0a0a2cf89"],
 "."
}], "ItemNumbered",
 CellChangeTimes->{{3.954747504802784*^9, 3.954747511349125*^9}, {
