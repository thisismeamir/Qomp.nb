  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"\[LeftAngleBracket]", 
         RowBox[{"\[CenterDot]", ",", "\[CenterDot]"}], 
         "\[RightAngleBracket]"}], TraditionalForm], "errors" -> {}, "input" -> 
