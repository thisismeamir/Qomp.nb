Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "Example", " ", "with", " ", "first", " ", "column", " ", "and", " ", 
    "last", " ", "row"}], " ", "*)"}], "\n", 
  RowBox[{
   RowBox[{
    RowBox[{"c", " ", "=", " ", 
     RowBox[{"{", 
      RowBox[{
      "1", ",", " ", "2", ",", " ", "3", ",", " ", "4", ",", " ", "5"}], 
      "}"}]}], ";"}], " ", 
   RowBox[{"(*", " ", 
    RowBox[{"First", " ", "column"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"r", " ", "=", " ", 
     RowBox[{"{", 
      RowBox[{
      "5", ",", " ", "6", ",", " ", "7", ",", " ", "8", ",", " ", "9"}], 
      "}"}]}], ";"}], " ", 
   RowBox[{"(*", " ", 
    RowBox[{"Last", " ", "row"}], " ", "*)"}], "\n", "\n", 
   RowBox[{
    RowBox[{"H", " ", "=", " ", 
     RowBox[{"HankelMatrix", "[", 
      RowBox[{"c", ",", " ", "r"}], "]"}]}], ";"}], "\n", 
   RowBox[{"Print", "[", "\"\<Hankel Matrix:\>\"", "]"}], "\n", 
   RowBox[{"MatrixForm", "[", "H", "]"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Alternative", " ", "implementation", " ", "directly", " ", "using", " ", 
     "the", " ", "property"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"HankelMatrixDirect", "[", "values_", "]"}], " ", ":=", " ", 
     RowBox[{"Table", "[", "\n", "  ", 
      RowBox[{
       RowBox[{"values", "[", 
        RowBox[{"[", 
         RowBox[{"i", " ", "+", " ", "j", " ", "-", " ", "1"}], "]"}], "]"}], 
       ",", "\n", "  ", 
       RowBox[{"{", 
        RowBox[{"i", ",", " ", "1", ",", " ", 
         RowBox[{"Ceiling", "[", 
          RowBox[{
           RowBox[{"Length", "[", "values", "]"}], "/", "2"}], "]"}]}], "}"}],
        ",", " ", "\n", "  ", 
       RowBox[{"{", 
        RowBox[{"j", ",", " ", "1", ",", " ", 
         RowBox[{"Ceiling", "[", 
          RowBox[{
           RowBox[{"Length", "[", "values", "]"}], "/", "2"}], "]"}]}], 
        "}"}]}], "\n", "]"}]}], ";"}], "\n", "\n", 
   RowBox[{
    RowBox[{"values", " ", "=", " ", 
     RowBox[{"{", 
      RowBox[{
      "1", ",", " ", "2", ",", " ", "3", ",", " ", "4", ",", " ", "5", ",", 
       " ", "6", ",", " ", "7", ",", " ", "8", ",", " ", "9"}], "}"}]}], 
    ";"}], "\n", 
   RowBox[{
    RowBox[{"H2", " ", "=", " ", 
     RowBox[{"HankelMatrixDirect", "[", "values", "]"}]}], ";"}], "\n", 
   RowBox[{
   "Print", "[", "\"\<Hankel Matrix from sequential values:\>\"", "]"}], "\n", 
   RowBox[{"MatrixForm", "[", "H2", "]"}], "\n", "\n", 
   RowBox[{
    RowBox[{"Print", "[", "\"\<Checking Hankel property:\>\"", "]"}], ";"}], 
   "\n", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"get", " ", "dimensions"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"{", 
      RowBox[{"nRows", ",", "nCols"}], "}"}], "=", 
     RowBox[{"Dimensions", "[", "H", "]"}]}], ";"}], "\n", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Group", " ", "elements", " ", "by", " ", 
     RowBox[{"(", 
      RowBox[{"i", "+", "j"}], ")"}]}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"antiDiagonals", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Select", "[", 
         RowBox[{
          RowBox[{"Flatten", "[", 
           RowBox[{
            RowBox[{"Table", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"i", ",", "j", ",", 
                RowBox[{"H", "[", 
                 RowBox[{"[", 
                  RowBox[{"i", ",", "j"}], "]"}], "]"}]}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"i", ",", "1", ",", "nRows"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"j", ",", "1", ",", "nCols"}], "}"}]}], "]"}], ",", 
            "1"}], "]"}], ",", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              RowBox[{"#", "[", 
               RowBox[{"[", "1", "]"}], "]"}], "+", 
              RowBox[{"#", "[", 
               RowBox[{"[", "2", "]"}], "]"}]}], "==", "k"}], ")"}], "&"}]}], 
         "]"}], "[", 
        RowBox[{"[", 
         RowBox[{"All", ",", "3"}], "]"}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", "2", ",", 
         RowBox[{"nRows", "+", "nCols"}]}], "}"}]}], "]"}]}], ";"}], "\n", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{"Check", " ", "if", " ", "each", " ", "anti"}], "-", 
     RowBox[{
     "diagonal", " ", "has", " ", "all", " ", "equal", " ", "elements"}]}], 
    "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"allAntiDiagsEqual", "=", 
     RowBox[{"And", "@@", 
      RowBox[{"Map", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"SameQ", "@@", "#"}], "&"}], ",", "antiDiagonals"}], 
       "]"}]}]}], ";"}], "\[IndentingNewLine]", "\n", 
   RowBox[{
    RowBox[{"Print", "[", 
     RowBox[{
     "\"\<All elements along each anti-diagonal are equal? \>\"", ",", 
      "allAntiDiagsEqual"}], "]"}], ";"}], "\n", "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Relationship", " ", "between", " ", "Hankel", " ", "and", " ", 
     "Toeplitz", " ", "matrices"}], " ", "*)"}], "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "A", " ", "Hankel", " ", "matrix", " ", "can", " ", "be", " ", 
     "converted", " ", "to", " ", "a", " ", "Toeplitz", " ", "matrix", " ", 
     "by", " ", "reversing", " ", "each", " ", "row"}], " ", "*)"}], "\n", 
   RowBox[{
   "Print", "[", "\"\<Converting Hankel to Toeplitz by row reversal:\>\"", 
    "]"}], "\n", 
   RowBox[{
    RowBox[{"HankelToToeplitz", " ", "=", " ", 
     RowBox[{"Map", "[", 
      RowBox[{"Reverse", ",", " ", "H"}], "]"}]}], ";"}], "\n", 
   RowBox[{"MatrixForm", "[", "HankelToToeplitz", "]"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<Is this a Toeplitz matrix? \>\"", ",", " ", "\n", "  ", 
     RowBox[{"And", " ", "@@", " ", 
      RowBox[{"Flatten", "[", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"HankelToToeplitz", "[", 
           RowBox[{"[", 
            RowBox[{"i", ",", " ", "j"}], "]"}], "]"}], " ", "==", " ", 
          RowBox[{"HankelToToeplitz", "[", 
           RowBox[{"[", 
            RowBox[{
             RowBox[{"i", "+", "1"}], ",", " ", 
             RowBox[{"j", "+", "1"}]}], "]"}], "]"}]}], ",", " ", "\n", 
         "    ", 
         RowBox[{"{", 
          RowBox[{"i", ",", " ", "1", ",", " ", 
           RowBox[{
            RowBox[{
             RowBox[{"Dimensions", "[", "HankelToToeplitz", "]"}], "[", 
             RowBox[{"[", "1", "]"}], "]"}], "-", "1"}]}], "}"}], ",", " ", 
         "\n", "    ", 
         RowBox[{"{", 
          RowBox[{"j", ",", " ", "1", ",", " ", 
           RowBox[{
            RowBox[{
             RowBox[{"Dimensions", "[", "HankelToToeplitz", "]"}], "[", 
             RowBox[{"[", "2", "]"}], "]"}], "-", "1"}]}], "}"}]}], "]"}], 
       "]"}]}]}], "]"}], "\n"}]}]], "Input",
