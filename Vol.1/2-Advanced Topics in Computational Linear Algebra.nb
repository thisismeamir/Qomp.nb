Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"Create", " ", "a", " ", "scale"}], "-", 
    RowBox[{"free", " ", "network", " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"common", " ", "in", " ", "real"}], "-", 
       RowBox[{"world", " ", "scenarios"}]}], ")"}]}]}], " ", "*)"}], "\n", 
  RowBox[{
   RowBox[{
    RowBox[{"n", " ", "=", " ", "100"}], ";"}], " ", 
   RowBox[{"(*", " ", 
    RowBox[{"Number", " ", "of", " ", "nodes"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"m", " ", "=", " ", "2"}], ";"}], "   ", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Number", " ", "of", " ", "edges", " ", "to", " ", "attach", " ", "from", 
     " ", "a", " ", "new", " ", "node", " ", "to", " ", "existing", " ", 
     "nodes"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"g", " ", "=", " ", 
     RowBox[{"RandomGraph", "[", 
      RowBox[{"BarabasiAlbertGraphDistribution", "[", 
       RowBox[{"n", ",", " ", "m"}], "]"}], "]"}]}], ";"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Get", " ", "the", " ", "adjacency", " ", "matrix"}], " ", "*)"}],
    "\n", 
