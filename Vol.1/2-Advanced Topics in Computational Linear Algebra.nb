      "}"}]}], ";"}], " ", 
   RowBox[{"(*", " ", 
    RowBox[{"First", " ", "row"}], " ", "*)"}], "\n", "\n", 
   RowBox[{
    RowBox[{"T", " ", "=", " ", 
     RowBox[{"ToeplitzMatrix", "[", 
      RowBox[{"c", ",", " ", "r"}], "]"}]}], ";"}], "\n", 
   RowBox[{"Print", "[", "\"\<Toeplitz Matrix:\>\"", "]"}], "\n", 
   RowBox[{"MatrixForm", "[", "T", "]"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Verify", " ", "the", " ", "Toeplitz", " ", "property"}], " ", 
    "*)"}], "\n", 
   RowBox[{"Print", "[", "\"\<Checking Toeplitz property:\>\"", "]"}], "\n", 
   RowBox[{
    RowBox[{"allDiags", " ", "=", " ", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"Diagonal", "[", 
        RowBox[{"T", ",", " ", "k"}], "]"}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"k", ",", " ", 
         RowBox[{"-", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Length", "[", "c", "]"}], " ", "-", " ", "1"}], ")"}]}], 
         ",", " ", 
         RowBox[{
          RowBox[{"Length", "[", "r", "]"}], " ", "-", " ", "1"}]}], "}"}]}], 
      "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"allDiagsEqual", " ", "=", " ", 
     RowBox[{"And", " ", "@@", " ", 
      RowBox[{"Map", "[", "\n", "  ", 
       RowBox[{
        RowBox[{
         RowBox[{"Apply", "[", 
          RowBox[{"Equal", ",", " ", "#"}], "]"}], " ", "&"}], ",", "\n", 
        "  ", "allDiags"}], "\n", "]"}]}]}], ";"}], "\n", 
