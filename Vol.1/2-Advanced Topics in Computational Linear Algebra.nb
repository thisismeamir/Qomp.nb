Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"Define", " ", "a", " ", "finite"}], "-", 
    RowBox[{
    "dimensional", " ", "representation", " ", "of", " ", "operators"}]}], 
   " ", "*)"}], "\n", 
  RowBox[{"(*", " ", 
   RowBox[{
   "Matrix", " ", "representation", " ", "of", " ", "a", " ", "diagonal", " ",
     "operator"}], " ", "*)"}], "\n", 
  RowBox[{
   RowBox[{
    RowBox[{"diagonalOperator", "[", "eigenvalues_List", "]"}], " ", ":=", 
    " ", 
    RowBox[{"DiagonalMatrix", "[", "eigenvalues", "]"}]}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Matrix", " ", "representation", " ", "of", " ", "a", " ", "shift", " ", 
     "operator"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"shiftOperator", "[", "n_Integer", "]"}], " ", ":=", " ", "\n", 
    "  ", 
    RowBox[{"SparseArray", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Band", "[", 
         RowBox[{"{", 
          RowBox[{"1", ",", " ", "2"}], "}"}], "]"}], " ", "->", " ", 
        RowBox[{"Table", "[", 
         RowBox[{"1", ",", " ", 
          RowBox[{"{", 
           RowBox[{"n", "-", "1"}], "}"}]}], "]"}]}], "}"}], ",", " ", 
      RowBox[{"{", 
       RowBox[{"n", ",", " ", "n"}], "}"}]}], "]"}]}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Matrix", " ", "representation", " ", "of", " ", "a", " ", 
     "multiplication", " ", "operator", " ", 
     RowBox[{"(", 
      RowBox[{"by", " ", "a", " ", "function"}], ")"}]}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"multiplicationOperator", "[", 
     RowBox[{"f_", ",", " ", "gridpoints_List"}], "]"}], " ", ":=", " ", 
    RowBox[{"DiagonalMatrix", "[", 
     RowBox[{"f", " ", "/@", " ", "gridpoints"}], "]"}]}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Check", " ", "if", " ", "an", " ", "operator", " ", "is", " ", "bounded",
      " ", 
     RowBox[{"(", 
      RowBox[{"for", " ", "finite", " ", "matrices"}], ")"}]}], " ", "*)"}], 
   "\n", 
   RowBox[{
    RowBox[{"isBounded", "[", 
     RowBox[{"mat_", "?", "MatrixQ"}], "]"}], " ", ":=", " ", "True"}], "  ", 
   
   RowBox[{"(*", " ", 
    RowBox[{
    "All", " ", "finite", " ", "matrices", " ", "represent", " ", "bounded", 
     " ", "operators"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"operatorNorm", "[", 
     RowBox[{"mat_", "?", "MatrixQ"}], "]"}], " ", ":=", " ", 
    RowBox[{"Max", "[", 
     RowBox[{"SingularValueList", "[", "mat", "]"}], "]"}]}], "  ", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"Operator", " ", "norm"}], " ", "=", " ", 
     RowBox[{"largest", " ", "singular", " ", "value"}]}], " ", "*)"}], "\n", 
   "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Example", " ", "operators"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"n", " ", "=", " ", "10"}], ";"}], "\n", 
   RowBox[{
    RowBox[{"D1", " ", "=", " ", 
     RowBox[{"diagonalOperator", "[", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"1", "/", "i"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"i", ",", " ", "1", ",", " ", "n"}], "}"}]}], "]"}], "]"}]}],
     ";"}], "  ", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Diagonal", " ", "operator", " ", "with", " ", "decreasing", " ", 
     "entries"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"S1", " ", "=", " ", 
     RowBox[{"shiftOperator", "[", "n", "]"}]}], ";"}], "  ", 
   RowBox[{"(*", " ", 
    RowBox[{"Right", " ", "shift", " ", "operator"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"grid", " ", "=", " ", 
     RowBox[{"Range", "[", 
      RowBox[{"0", ",", " ", "1", ",", " ", 
       RowBox[{"1", "/", 
        RowBox[{"(", 
         RowBox[{"n", "-", "1"}], ")"}]}]}], "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"M1", " ", "=", " ", 
     RowBox[{"multiplicationOperator", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Sin", "[", 
         RowBox[{"2", " ", "Pi", " ", "#"}], "]"}], "&"}], ",", " ", "grid"}],
       "]"}]}], ";"}], "  ", 
   RowBox[{"(*", " ", 
    RowBox[{"Multiplication", " ", "by", " ", "sin", 
     RowBox[{"(", 
      RowBox[{"2", "\[Pi]x"}], ")"}]}], " ", "*)"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Display", " ", "the", " ", "operators"}], " ", "*)"}], "\n", 
