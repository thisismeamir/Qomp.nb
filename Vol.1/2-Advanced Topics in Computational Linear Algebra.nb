Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"Create", " ", "a", " ", "matrix"}], " ", "*)"}], "\n", 
  RowBox[{
   RowBox[{
    RowBox[{"A", " ", "=", " ", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"1", ",", " ", "2"}], "}"}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"3", ",", " ", "4"}], "}"}]}], "}"}]}], ";"}], "\n", 
   RowBox[{"Print", "[", "\"\<Matrix A:\>\"", "]"}], "\n", 
   RowBox[{"MatrixForm", "[", "A", "]"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Define", " ", "some", " ", "matrix", " ", "functions", " ", "using", " ",
      "MatrixFunction"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"sinA", " ", "=", " ", 
     RowBox[{"MatrixFunction", "[", 
      RowBox[{"Sin", ",", " ", "A"}], "]"}]}], ";"}], "\n", 
   RowBox[{"Print", "[", "\"\<sin(A):\>\"", "]"}], "\n", 
   RowBox[{"MatrixForm", "[", "sinA", "]"}], "\n", "\n", 
   RowBox[{
    RowBox[{"cosA", " ", "=", " ", 
     RowBox[{"MatrixFunction", "[", 
      RowBox[{"Cos", ",", " ", "A"}], "]"}]}], ";"}], "\n", 
   RowBox[{"Print", "[", "\"\<cos(A):\>\"", "]"}], "\n", 
   RowBox[{"MatrixForm", "[", "cosA", "]"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{
      RowBox[{"Verify", " ", "the", " ", "identity", " ", 
       RowBox[{"sin", "^", "2"}], 
       RowBox[{"(", "A", ")"}]}], " ", "+", " ", 
      RowBox[{
       RowBox[{"cos", "^", "2"}], 
       RowBox[{"(", "A", ")"}]}]}], " ", "=", " ", "I"}], " ", "*)"}], "\n", 
   RowBox[{"Print", "[", "\"\<sin^2(A) + cos^2(A):\>\"", "]"}], "\n", 
   RowBox[{"MatrixForm", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"sinA", ".", "sinA"}], " ", "+", " ", 
      RowBox[{"cosA", ".", "cosA"}]}], " ", "//", " ", "Chop"}], "]"}], "\n", 
   "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Using", " ", "Diagonalization", " ", "approach"}], " ", "*)"}], 
   "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"{", 
      RowBox[{"eigenvalues", ",", " ", "P"}], "}"}], " ", "=", " ", 
     RowBox[{"Eigensystem", "[", "A", "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"Diag", " ", "=", " ", 
     RowBox[{"DiagonalMatrix", "[", "eigenvalues", "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"PInv", " ", "=", " ", 
     RowBox[{"Inverse", "[", 
      RowBox[{"Transpose", "[", "P", "]"}], "]"}]}], ";"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Computing", " ", "sin", 
     RowBox[{"(", "A", ")"}], " ", "via", " ", "diagonalization"}], " ", 
    "*)"}], "\n", 
   RowBox[{
    RowBox[{"sinD", " ", "=", " ", 
     RowBox[{"DiagonalMatrix", "[", 
      RowBox[{"Map", "[", 
       RowBox[{"Sin", ",", " ", "eigenvalues"}], "]"}], "]"}]}], ";"}], "\n", 
   
   RowBox[{
    RowBox[{"sinA_diag", " ", "=", " ", 
     RowBox[{"P", ".", "sinD", ".", "PInv"}]}], ";"}], "\n", 
   RowBox[{"Print", "[", "\"\<sin(A) via diagonalization:\>\"", "]"}], "\n", 
   RowBox[{"MatrixForm", "[", "sinA_diag", "]"}], "\n", 
   RowBox[{"Print", "[", "\"\<Difference with direct computation:\>\"", "]"}],
    "\n", 
   RowBox[{"MatrixForm", "[", 
    RowBox[{
     RowBox[{"sinA", " ", "-", " ", "sinA_diag"}], " ", "//", " ", "Chop"}], 
    "]"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Define", " ", "a", " ", "more", " ", "complex", " ", "function"}], " ", 
    "*)"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"f", "[", "x_", "]"}], " ", ":=", " ", 
     RowBox[{
      RowBox[{"Exp", "[", 
       RowBox[{"-", 
        RowBox[{"x", "^", "2"}]}], "]"}], " ", "*", " ", 
      RowBox[{"Sin", "[", "x", "]"}]}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"fA", " ", "=", " ", 
     RowBox[{"MatrixFunction", "[", 
      RowBox[{"f", ",", " ", "A"}], "]"}]}], ";"}], "\n", 
   RowBox[{"Print", "[", "\"\<f(A) = e^(-A^2) * sin(A):\>\"", "]"}], "\n", 
   RowBox[{"MatrixForm", "[", "fA", "]"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Computing", " ", "a", " ", "matrix", " ", "sign", " ", "function"}], " ",
     "*)"}], "\n", 
   RowBox[{
    RowBox[{"signA", " ", "=", " ", 
     RowBox[{"MatrixFunction", "[", 
      RowBox[{"Sign", ",", " ", "A"}], "]"}]}], ";"}], "\n", 
   RowBox[{"Print", "[", "\"\<sign(A):\>\"", "]"}], "\n", 
   RowBox[{"MatrixForm", "[", "signA", "]"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"Verify", " ", "sign", 
      RowBox[{
       RowBox[{"(", "A", ")"}], "^", "2"}]}], " ", "=", " ", "I"}], " ", 
    "*)"}], "\n", 
   RowBox[{"Print", "[", "\"\<sign(A)^2:\>\"", "]"}], "\n", 
   RowBox[{"MatrixForm", "[", 
    RowBox[{
     RowBox[{"signA", ".", "signA"}], " ", "//", " ", "Chop"}], "]"}], 
   "\n"}]}]], "Input",
