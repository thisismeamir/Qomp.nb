Cell[TextData[{
 "A circulant matrix is a special type of Toeplitz matrix where each row is a \
cyclic shift of the row above it. For an ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["n", "TI"], "\[Cross]", 
         StyleBox["n", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "n \\times n", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "5f3b2513-d51a-4d86-82e7-8fb357cc3f06"],
 " circulant matrix ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       StyleBox["C", "TI"], TraditionalForm], "errors" -> {}, "input" -> "C", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "ac968bd8-6a08-4b2e-a586-026affd29b69"],
 ", the entries satisfy:\n\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["C", "TI"], 
          RowBox[{
            StyleBox["i", "TI"], ",", 
            StyleBox["j", "TI"]}]], "\[LongEqual]", 
         SubscriptBox[
          StyleBox["c", "TI"], 
          RowBox[{
            RowBox[{"(", 
              RowBox[{
                StyleBox["j", "TI"], "-", 
                StyleBox["i", "TI"]}], ")"}], "mod", 
            StyleBox["n", "TI"]}]]}], TraditionalForm], "errors" -> {}, 
     "input" -> "C_{i,j} = c_{(j-i) \\bmod n}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "6cd5a004-18f3-48e5-b920-2f92a757f4e3"],
