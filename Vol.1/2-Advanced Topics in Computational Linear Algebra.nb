Cell[CellGroupData[{

Cell["Unbounded Operators", "Section",
 CellChangeTimes->{{3.95481710743832*^9, 
  3.9548171086156063`*^9}},ExpressionUUID->"def35f97-3d8d-432d-b2f8-\
d9d4e1b2b84c"],

Cell[TextData[{
 "Unbounded operators are crucial in quantum mechanics and differential \
equations but bring additional complexity as well. An operator ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       StyleBox["T", "TI"], TraditionalForm], "errors" -> {}, "input" -> "T", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "aba19347-315b-425f-b3dc-7fbd002ed424"],
 " is unbounded if it is not bounded, meaning for any constant ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       StyleBox["M", "TI"], TraditionalForm], "errors" -> {}, "input" -> "M", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "76f98e4a-b3e4-4949-8179-9a2292dfe331"],
 ": ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"||", 
         StyleBox["T", "TI"], 
         RowBox[{"(", 
           StyleBox["x", "TI"], ")"}], "||", ">", 
         StyleBox["M", "TI"], "\[CenterDot]", "||", 
         StyleBox["x", "TI"], "||"}], TraditionalForm], "errors" -> {}, 
     "input" -> "||T(x)|| > M\\cdot ||x||", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "30ab2dfe-f083-4f40-b52b-2b5fa3473e6a"],
 ". They are not continuous and are only defined on a dense subset of the \
Hilbert space. Common examples are the differentiation and some integration \
operators."
}], "Text",
 CellChangeTimes->{{3.954817120750168*^9, 
  3.954817224329311*^9}},ExpressionUUID->"93d8fca0-0c10-44b0-bcc2-\
8b43cb67db8a"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"Implement", " ", "a", " ", "finite"}], "-", 
    RowBox[{
    "dimensional", " ", "approximation", " ", "of", " ", "the", " ", 
     "differentiation", " ", "operator"}]}], "*)"}], 
  RowBox[{
   RowBox[{
    RowBox[{"differentiationOperator", "[", 
     RowBox[{"n_Integer", ",", 
      RowBox[{"domainLength_", ":", "1"}]}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"h", "=", 
         RowBox[{"domainLength", "/", 
          RowBox[{"(", 
           RowBox[{"n", "-", "1"}], ")"}]}]}], ",", "mat"}], "}"}], ",", 
      RowBox[{
       RowBox[{"mat", "=", 
        RowBox[{"SparseArray", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{"{", 
              RowBox[{"i_", ",", "i_"}], "}"}], "->", "0"}], ",", 
            RowBox[{
             RowBox[{
              RowBox[{"{", 
               RowBox[{"i_", ",", "j_"}], "}"}], "/;", 
              RowBox[{"j", "==", 
               RowBox[{"i", "+", "1"}]}]}], "->", 
             RowBox[{"1", "/", "h"}]}], ",", 
            RowBox[{
             RowBox[{
              RowBox[{"{", 
               RowBox[{"i_", ",", "j_"}], "}"}], "/;", 
              RowBox[{"j", "==", 
               RowBox[{"i", "-", "1"}]}]}], "->", 
             RowBox[{
              RowBox[{"-", "1"}], "/", "h"}]}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"n", ",", "n"}], "}"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"mat", "[", 
         RowBox[{"[", 
          RowBox[{"1", ",", "1"}], "]"}], "]"}], "=", 
        RowBox[{
         RowBox[{"-", "1"}], "/", "h"}]}], ";", 
       RowBox[{"(*", 
        RowBox[{"Forward", " ", "difference", " ", "at", " ", "boundary"}], 
        "*)"}], 
       RowBox[{
        RowBox[{"mat", "[", 
         RowBox[{"[", 
          RowBox[{"1", ",", "2"}], "]"}], "]"}], "=", 
        RowBox[{"1", "/", "h"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"mat", "[", 
         RowBox[{"[", 
          RowBox[{"n", ",", 
           RowBox[{"n", "-", "1"}]}], "]"}], "]"}], "=", 
        RowBox[{
         RowBox[{"-", "1"}], "/", "h"}]}], ";", 
       RowBox[{"(*", 
        RowBox[{"Backward", " ", "difference", " ", "at", " ", "boundary"}], 
        "*)"}], 
       RowBox[{
        RowBox[{"mat", "[", 
         RowBox[{"[", 
          RowBox[{"n", ",", "n"}], "]"}], "]"}], "=", 
        RowBox[{"1", "/", "h"}]}], ";", "\[IndentingNewLine]", "mat"}]}], 
     "]"}]}], "\n", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Second", " ", "derivative", " ", "operator"}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"laplacianOperator", "[", 
     RowBox[{"n_Integer", ",", 
      RowBox[{"domainLength_", ":", "1"}]}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"h", "=", 
         RowBox[{"domainLength", "/", 
          RowBox[{"(", 
           RowBox[{"n", "-", "1"}], ")"}]}]}], ",", "mat"}], "}"}], ",", 
      RowBox[{
       RowBox[{"mat", "=", 
        RowBox[{"SparseArray", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{"{", 
              RowBox[{"i_", ",", "i_"}], "}"}], "->", 
             RowBox[{
              RowBox[{"-", "2"}], "/", 
              RowBox[{"h", "^", "2"}]}]}], ",", 
            RowBox[{
             RowBox[{
              RowBox[{"{", 
               RowBox[{"i_", ",", "j_"}], "}"}], "/;", 
              RowBox[{"j", "==", 
               RowBox[{"i", "+", "1"}]}]}], "->", 
             RowBox[{"1", "/", 
              RowBox[{"h", "^", "2"}]}]}], ",", 
            RowBox[{
             RowBox[{
              RowBox[{"{", 
               RowBox[{"i_", ",", "j_"}], "}"}], "/;", 
              RowBox[{"j", "==", 
               RowBox[{"i", "-", "1"}]}]}], "->", 
             RowBox[{"1", "/", 
              RowBox[{"h", "^", "2"}]}]}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"n", ",", "n"}], "}"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{
        "Adjust", " ", "boundary", " ", "conditions", " ", "for", " ", 
         "Dirichlet"}], "*)"}], 
       RowBox[{
        RowBox[{"mat", "[", 
         RowBox[{"[", 
          RowBox[{"1", ",", "1"}], "]"}], "]"}], "=", "1"}], ";", 
       RowBox[{"(*", 
        RowBox[{
         RowBox[{"u", 
          RowBox[{"(", "0", ")"}]}], "=", "0"}], "*)"}], 
       RowBox[{
        RowBox[{"mat", "[", 
         RowBox[{"[", 
          RowBox[{"1", ",", "2"}], "]"}], "]"}], "=", "0"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"mat", "[", 
         RowBox[{"[", 
          RowBox[{"n", ",", 
           RowBox[{"n", "-", "1"}]}], "]"}], "]"}], "=", "0"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"mat", "[", 
         RowBox[{"[", 
          RowBox[{"n", ",", "n"}], "]"}], "]"}], "=", "1"}], ";", 
       RowBox[{"(*", 
        RowBox[{
         RowBox[{"u", 
          RowBox[{"(", "1", ")"}]}], "=", "0"}], "*)"}], "mat"}]}], "]"}]}], 
   "\n", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Example", " ", "usage"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"n", "=", "50"}], ";"}], "\n", 
   RowBox[{
    RowBox[{"dop", "=", 
     RowBox[{"differentiationOperator", "[", "n", "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"lop", "=", 
     RowBox[{"laplacianOperator", "[", "n", "]"}]}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Visualize", " ", "the", " ", "operators"}], "*)"}], "\n", 
   RowBox[{"MatrixPlot", "[", 
    RowBox[{"dop", ",", 
     RowBox[{"PlotLabel", "->", "\"\<Differentiation Operator\>\""}]}], "]"}],
    "\n", 
   RowBox[{"MatrixPlot", "[", 
    RowBox[{"lop", ",", 
     RowBox[{"PlotLabel", "->", "\"\<Laplacian Operator\>\""}]}], "]"}], "\n",
    "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Test", " ", "functions"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"x", "=", 
     RowBox[{"Range", "[", 
      RowBox[{"0", ",", "1", ",", 
       RowBox[{"1", "/", 
        RowBox[{"(", 
         RowBox[{"n", "-", "1"}], ")"}]}]}], "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"f1", "=", 
     RowBox[{"Sin", "[", 
      RowBox[{"2", " ", "Pi", " ", "x"}], "]"}]}], ";"}], "  ", 
   RowBox[{"(*", 
    RowBox[{"Sin", 
     RowBox[{"(", 
      RowBox[{"2", "\[Pi]x"}], ")"}]}], "*)"}], "\n", 
   RowBox[{
    RowBox[{"f2", "=", 
     RowBox[{"Exp", "[", 
      RowBox[{
       RowBox[{"-", "10"}], " ", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"x", "-", "0.5"}], ")"}], "^", "2"}]}], "]"}]}], ";"}], "  ", 
   RowBox[{"(*", 
    RowBox[{"Gaussian", " ", "peak"}], "*)"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Apply", " ", "operators"}], "*)"}], "\n", 
   RowBox[{
    RowBox[{"Df1", "=", 
     RowBox[{"dop", ".", "f1"}]}], ";"}], "  ", 
   RowBox[{"(*", 
    RowBox[{"Approximate", " ", "derivative", " ", "of", " ", "sin", 
     RowBox[{"(", 
      RowBox[{"2", "\[Pi]x"}], ")"}]}], "*)"}], "\n", 
   RowBox[{
    RowBox[{"Df2", "=", 
     RowBox[{"dop", ".", "f2"}]}], ";"}], "  ", 
   RowBox[{"(*", 
    RowBox[{"Approximate", " ", "derivative", " ", "of", " ", "Gaussian"}], 
    "*)"}], "\n", 
   RowBox[{
    RowBox[{"Lf1", "=", 
     RowBox[{"lop", ".", "f1"}]}], ";"}], "  ", 
   RowBox[{"(*", 
    RowBox[{"Approximate", " ", "Laplacian", " ", "of", " ", "sin", 
     RowBox[{"(", 
      RowBox[{"2", "\[Pi]x"}], ")"}]}], "*)"}], "\n", 
   RowBox[{
    RowBox[{"Lf2", "=", 
     RowBox[{"lop", ".", "f2"}]}], ";"}], "  ", 
   RowBox[{"(*", 
    RowBox[{"Approximate", " ", "Laplacian", " ", "of", " ", "Gaussian"}], 
    "*)"}], "\n", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Expected", " ", "analytical", " ", "results"}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"exactDf1", "=", 
     RowBox[{"2", " ", "Pi", " ", 
      RowBox[{"Cos", "[", 
       RowBox[{"2", " ", "Pi", " ", "x"}], "]"}]}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"exactDf2", "=", 
     RowBox[{
      RowBox[{"-", "20"}], " ", 
      RowBox[{"(", 
       RowBox[{"x", "-", "0.5"}], ")"}], " ", 
      RowBox[{"Exp", "[", 
       RowBox[{
        RowBox[{"-", "10"}], " ", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"x", "-", "0.5"}], ")"}], "^", "2"}]}], "]"}]}]}], ";"}], 
   "\n", 
   RowBox[{
    RowBox[{"exactLf1", "=", 
     RowBox[{
      RowBox[{"-", "4"}], " ", 
      RowBox[{"Pi", "^", "2"}], " ", 
      RowBox[{"Sin", "[", 
       RowBox[{"2", " ", "Pi", " ", "x"}], "]"}]}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"exactLf2", "=", 
     RowBox[{
      RowBox[{"(", 
