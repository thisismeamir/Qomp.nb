   RowBox[{
    RowBox[{"Print", "[", 
     RowBox[{
     "\"\<Approximate derivative of Gaussian and exact result:\>\"", ",", " ",
       "Df2", ",", " ", "exactDf2"}], "]"}], ";"}], "\[IndentingNewLine]", 
   GridBox[{
     {
      RowBox[{
       RowBox[{"Print", "[", 
        RowBox[{
        "\"\<Approximate laplacian of Sin(2\[Pi]x) and exact result:\>\"", 
         ",", " ", "Lf1", ",", " ", "exactLf1"}], "]"}], ";"}]},
