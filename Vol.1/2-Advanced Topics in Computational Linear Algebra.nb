Cell[CellGroupData[{

Cell["Sparse Matrix in Mathematica", "Subsection",
 CellChangeTimes->{{3.954812509918953*^9, 
  3.9548125164306717`*^9}},ExpressionUUID->"ca4af1e8-6ee7-4c65-81c4-\
7971a3ac7c8c"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"Create", " ", "a", " ", "sparse", " ", "matrix"}], " ", "*)"}], 
  "\n", 
  RowBox[{
   RowBox[{
    RowBox[{"n", " ", "=", " ", "1000"}], ";"}], "\n", 
   RowBox[{
    RowBox[{"density", " ", "=", " ", "0.01"}], ";"}], " ", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"1", "%", " ", "of", " ", "elements", " ", "are", " ", "non"}], 
     "-", "zero"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"nnz", " ", "=", " ", 
     RowBox[{"Round", "[", 
      RowBox[{
       RowBox[{"n", "^", "2"}], " ", "*", " ", "density"}], "]"}]}], ";"}], 
   " ", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"Number", " ", "of", " ", "non"}], "-", 
     RowBox[{"zero", " ", "elements"}]}], " ", "*)"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{
     "Random", " ", "sparse", " ", "matrix", " ", "using", " ", "built"}], 
     "-", 
     RowBox[{"in", " ", "SparseArray"}]}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"positions", " ", "=", " ", 
     RowBox[{"RandomInteger", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"1", ",", " ", "n"}], "}"}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"nnz", ",", " ", "2"}], "}"}]}], "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"values", " ", "=", " ", 
     RowBox[{"RandomReal", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"-", "1"}], ",", " ", "1"}], "}"}], ",", " ", "nnz"}], 
      "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"sparseMatrix", " ", "=", " ", 
     RowBox[{"SparseArray", "[", 
      RowBox[{
       RowBox[{"positions", " ", "->", " ", "values"}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"n", ",", " ", "n"}], "}"}]}], "]"}]}], ";"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Information", " ", "about", " ", "the", " ", "sparse", " ", "matrix"}], 
    " ", "*)"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<Matrix dimensions: \>\"", ",", " ", 
     RowBox[{"Dimensions", "[", "sparseMatrix", "]"}]}], "]"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<Number of non-zero elements: \>\"", ",", " ", 
     RowBox[{"Length", "[", 
      RowBox[{"sparseMatrix", "[", "\"\<NonzeroValues\>\"", "]"}], "]"}]}], 
    "]"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<Density: \>\"", ",", " ", 
     RowBox[{
      RowBox[{
       RowBox[{"Length", "[", 
        RowBox[{"sparseMatrix", "[", "\"\<NonzeroValues\>\"", "]"}], "]"}], 
       "/", 
       RowBox[{"(", 
        RowBox[{"n", "^", "2"}], ")"}]}], " ", "*", " ", "100"}], ",", " ", 
     "\"\<%\>\""}], "]"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Demonstrate", " ", "storage", " ", "efficiency"}], " ", "*)"}], 
   "\n", 
   RowBox[{
    RowBox[{"bytesRequired", " ", "=", " ", 
     RowBox[{"ByteCount", "[", "sparseMatrix", "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"denseVersion", " ", "=", " ", 
     RowBox[{"Normal", "[", "sparseMatrix", "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"denseBytesRequired", " ", "=", " ", 
     RowBox[{"ByteCount", "[", "denseVersion", "]"}]}], ";"}], "\n", "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<Memory usage (sparse): \>\"", ",", " ", 
     RowBox[{"bytesRequired", "/", 
      RowBox[{"1024", "^", "2"}]}], ",", " ", "\"\< MB\>\""}], "]"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<Memory usage (dense): \>\"", ",", " ", 
     RowBox[{"denseBytesRequired", "/", 
      RowBox[{"1024", "^", "2"}]}], ",", " ", "\"\< MB\>\""}], "]"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<Compression ratio: \>\"", ",", " ", 
     RowBox[{"N", "[", 
      RowBox[{"denseBytesRequired", "/", "bytesRequired"}], "]"}]}], "]"}], 
   "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Manual", " ", "implementation", " ", "of", " ", "storage", " ", 
     "formats"}], " ", "*)"}], "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"COO", " ", "format"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"cooValues", " ", "=", " ", 
     RowBox[{"sparseMatrix", "[", "\"\<NonzeroValues\>\"", "]"}]}], ";"}], 
   "\n", 
   RowBox[{
    RowBox[{"cooPositions", " ", "=", " ", 
     RowBox[{"sparseMatrix", "[", "\"\<NonzeroPositions\>\"", "]"}]}], ";"}], 
   "\n", 
   RowBox[{
    RowBox[{"cooRows", " ", "=", " ", 
     RowBox[{"cooPositions", "[", 
      RowBox[{"[", 
       RowBox[{"All", ",", " ", "1"}], "]"}], "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"cooCols", " ", "=", " ", 
     RowBox[{"cooPositions", "[", 
      RowBox[{"[", 
       RowBox[{"All", ",", " ", "2"}], "]"}], "]"}]}], ";"}], "\n", "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<COO format storage size: \>\"", ",", " ", "\n", "  ", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"ByteCount", "[", "cooValues", "]"}], " ", "+", " ", 
        RowBox[{"ByteCount", "[", "cooRows", "]"}], " ", "+", " ", 
        RowBox[{"ByteCount", "[", "cooCols", "]"}]}], ")"}], "/", 
      RowBox[{"1024", "^", "2"}]}], ",", " ", "\"\< MB\>\""}], "]"}], "\n", 
   "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"CSR", " ", "format", " ", "simulation"}], " ", "*)"}], "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{
     "Sort", " ", "by", " ", "row", " ", "then", " ", "column", " ", "for", 
      " ", "CSR"}], "-", 
     RowBox[{"like", " ", "order"}]}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"sortedOrder", " ", "=", " ", 
     RowBox[{"Ordering", "[", 
      RowBox[{
       RowBox[{"cooRows", "*", "n"}], " ", "+", " ", "cooCols"}], "]"}]}], 
    ";"}], "\n", 
   RowBox[{
    RowBox[{"csrValues", " ", "=", " ", 
     RowBox[{"cooValues", "[", 
      RowBox[{"[", "sortedOrder", "]"}], "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"csrCols", " ", "=", " ", 
     RowBox[{"cooCols", "[", 
      RowBox[{"[", "sortedOrder", "]"}], "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"csrRowPtr", " ", "=", " ", 
     RowBox[{"Prepend", "[", 
      RowBox[{
       RowBox[{"Accumulate", "[", 
        RowBox[{
         RowBox[{"Tally", "[", "cooRows", "]"}], "[", 
         RowBox[{"[", 
          RowBox[{"All", ",", " ", "2"}], "]"}], "]"}], "]"}], ",", " ", 
       "0"}], "]"}]}], ";"}], "\n", "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<CSR format storage size: \>\"", ",", " ", "\n", "  ", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"ByteCount", "[", "csrValues", "]"}], " ", "+", " ", 
        RowBox[{"ByteCount", "[", "csrCols", "]"}], " ", "+", " ", 
        RowBox[{"ByteCount", "[", "csrRowPtr", "]"}]}], ")"}], "/", 
      RowBox[{"1024", "^", "2"}]}], ",", " ", "\"\< MB\>\""}], "]"}], "\n", 
   "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Sparse", " ", "matrix", " ", "operations"}], " ", "*)"}], "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Matrix", "-", 
     RowBox[{"Vector", " ", "multiplication"}]}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"v", " ", "=", " ", 
     RowBox[{"RandomReal", "[", 
      RowBox[{"1", ",", " ", "n"}], "]"}]}], ";"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Time", " ", "comparison"}], " ", "*)"}], "\n", 
   RowBox[{
   "Print", "[", "\"\<Sparse matrix-vector multiplication time:\>\"", "]"}], 
   "\n", 
   RowBox[{
    RowBox[{"sparseTime", " ", "=", " ", 
     RowBox[{"First", "[", 
      RowBox[{"Timing", "[", 
       RowBox[{
        RowBox[{"sparseResult", " ", "=", " ", 
         RowBox[{"sparseMatrix", ".", "v"}]}], ";"}], "]"}], "]"}]}], ";"}], 
   "\n", 
   RowBox[{"Print", "[", 
    RowBox[{
    "\"\<  Sparse: \>\"", ",", " ", "sparseTime", ",", " ", 
     "\"\< seconds\>\""}], "]"}], "\n", 
   RowBox[{
    RowBox[{"denseTime", " ", "=", " ", 
     RowBox[{"First", "[", 
      RowBox[{"Timing", "[", 
       RowBox[{
        RowBox[{"denseResult", " ", "=", " ", 
         RowBox[{"denseVersion", ".", "v"}]}], ";"}], "]"}], "]"}]}], ";"}], 
   "\n", 
   RowBox[{"Print", "[", 
    RowBox[{
    "\"\<  Dense: \>\"", ",", " ", "denseTime", ",", " ", 
     "\"\< seconds\>\""}], "]"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<  Speedup: \>\"", ",", " ", 
     RowBox[{"denseTime", "/", "sparseTime"}], ",", " ", "\"\<x\>\""}], "]"}],
    "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Accuracy", " ", "verification"}], " ", "*)"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{
    "\"\<Max difference between sparse and dense multiplication: \>\"", ",", 
     " ", "\n", "  ", 
     RowBox[{"Max", "[", 
      RowBox[{"Abs", "[", 
       RowBox[{"sparseResult", " ", "-", " ", "denseResult"}], "]"}], "]"}]}],
     "]"}]}]}]], "Input",
