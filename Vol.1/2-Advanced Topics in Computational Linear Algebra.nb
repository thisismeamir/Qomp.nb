Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "Simulate", " ", "a", " ", "large", " ", "linear", " ", "programming", " ",
     "problem"}], " ", "*)"}], "\n", 
  RowBox[{
   RowBox[{
    RowBox[{"nVariables", " ", "=", " ", "500"}], ";"}], "\n", 
   RowBox[{
    RowBox[{"nConstraints", " ", "=", " ", "300"}], ";"}], "\n", 
   RowBox[{
    RowBox[{"constraintsPerVariable", " ", "=", " ", "50"}], ";"}], " ", 
   RowBox[{"(*", " ", 
    RowBox[{"Each", " ", "variable", " ", "appears", " ", 
     RowBox[{"in", " ", "~", "5"}], " ", "constraints"}], " ", "*)"}], "\n", 
   "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Generate", " ", "random", " ", "sparse", " ", "constraint", " ", 
     "matrix"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"constraintMatrix", " ", "=", " ", 
     RowBox[{"SparseArray", "[", 
      RowBox[{
       RowBox[{"{", "}"}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"nConstraints", ",", " ", "nVariables"}], "}"}]}], "]"}]}], 
    ";"}], "\n", "\n", 
   RowBox[{"For", "[", 
    RowBox[{
     RowBox[{"var", " ", "=", " ", "1"}], ",", " ", 
     RowBox[{"var", " ", "<=", " ", "nVariables"}], ",", " ", 
     RowBox[{"var", "++"}], ",", "\n", "  ", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Select", " ", "random", " ", "constraints", " ", "that", " ", 
       "involve", " ", "this", " ", "variable"}], " ", "*)"}], "\n", "  ", 
     RowBox[{
      RowBox[{"varConstraints", " ", "=", " ", 
       RowBox[{"RandomSample", "[", 
        RowBox[{
         RowBox[{"Range", "[", "nConstraints", "]"}], ",", " ", 
         RowBox[{"RandomInteger", "[", 
          RowBox[{"{", 
           RowBox[{"1", ",", " ", 
            RowBox[{"constraintsPerVariable", " ", "*", " ", "2"}]}], "}"}], 
          "]"}]}], "]"}]}], ";", "\n", "  ", "\n", "  ", 
      RowBox[{"(*", " ", 
       RowBox[{"Assign", " ", "random", " ", "coefficients"}], " ", "*)"}], 
      "\n", "  ", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", " ", "=", " ", "1"}], ",", " ", 
        RowBox[{"i", " ", "<=", " ", 
         RowBox[{"Length", "[", "varConstraints", "]"}]}], ",", " ", 
        RowBox[{"i", "++"}], ",", "\n", "    ", 
        RowBox[{
         RowBox[{"constraint", " ", "=", " ", 
