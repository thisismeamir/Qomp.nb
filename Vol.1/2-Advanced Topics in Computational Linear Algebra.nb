Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"Create", " ", "a", " ", "sparse", " ", "matrix"}], " ", "*)"}], 
  "\n", 
  RowBox[{
   RowBox[{
    RowBox[{"n", " ", "=", " ", "1000"}], ";"}], "\n", 
   RowBox[{
    RowBox[{"density", " ", "=", " ", "0.01"}], ";"}], " ", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"1", "%", " ", "of", " ", "elements", " ", "are", " ", "non"}], 
     "-", "zero"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"nnz", " ", "=", " ", 
     RowBox[{"Round", "[", 
      RowBox[{
       RowBox[{"n", "^", "2"}], " ", "*", " ", "density"}], "]"}]}], ";"}], 
   " ", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"Number", " ", "of", " ", "non"}], "-", 
     RowBox[{"zero", " ", "elements"}]}], " ", "*)"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{
     "Random", " ", "sparse", " ", "matrix", " ", "using", " ", "built"}], 
     "-", 
     RowBox[{"in", " ", "SparseArray"}]}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"positions", " ", "=", " ", 
     RowBox[{"RandomInteger", "[", 
      RowBox[{
       RowBox[{"{", 
