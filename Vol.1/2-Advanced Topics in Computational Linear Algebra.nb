Cell[CellGroupData[{

Cell["Generalized Inverse (Moore-Penrose Inverse)", "Section",
 CellChangeTimes->{{3.95085934847407*^9, 
  3.950859361546915*^9}},ExpressionUUID->"7dda5873-350a-405a-8739-\
f30df0be474e"],

Cell[TextData[{
 "The Moore-Penrose inverse (or pseudoinverse) extends the concept of matrix \
inversion to non-square and singular matrices. For any matrix ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["A", "TI"], "\[Element]", 
         SuperscriptBox[
          StyleBox["C", "TI"], 
          RowBox[{
            StyleBox["m", "TI"], "\[Cross]", 
            StyleBox["n", "TI"]}]]}], TraditionalForm], "errors" -> {}, 
     "input" -> "A\\in C^{m\\times n}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "5f04def6-f288-46e0-8522-f490b43f5438"],
 ", the Moore-Penrose inverse ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       SuperscriptBox[
        StyleBox["A", "TI"], "+"], TraditionalForm], "errors" -> {}, "input" -> 
     "A^+", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "df0cf225-bbf7-416c-abcf-b62f2304534d"],
 " is the unique matrix satisfying all four Moore-Penrose conditions:"
}], "Text",
 CellChangeTimes->{{3.954724245064124*^9, 
  3.954724314110353*^9}},ExpressionUUID->"cde8d82d-3288-4e77-bc34-\
41cdadb17520"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SuperscriptBox[
          StyleBox["A", "TI"], "+"], 
         StyleBox["A", "TI"], 
         SuperscriptBox[
          StyleBox["A", "TI"], "+"], "\[LongEqual]", 
         SuperscriptBox[
          StyleBox["A", "TI"], "+"]}], TraditionalForm], "errors" -> {}, 
     "input" -> "A^+AA^+ = A^+", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "037cf82d-cf18-445a-8f57-d26bc4b03b9e"],
 " , and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["A", "TI"], 
         SuperscriptBox[
          StyleBox["A", "TI"], "+"], 
         StyleBox["A", "TI"], "\[LongEqual]", 
         StyleBox["A", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "AA^+A = A", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "e8365c3f-374a-44f8-90f1-29cc6455c73a"],
 " (reflexivity)."
}], "Item",
 CellChangeTimes->{{3.9547225644921722`*^9, 3.9547225700956697`*^9}, {
  3.954723058848873*^9, 3.954723074043437*^9}, {3.954723611466197*^9, 
  3.9547236200071697`*^9}, {3.954724325663045*^9, 
  3.9547243627397127`*^9}},ExpressionUUID->"1b2195f5-32f4-4ea1-93ea-\
de70b8f045ed"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
            RowBox[{
              StyleBox["A", "TI"], 
              SuperscriptBox[
               StyleBox["A", "TI"], "+"]}], ")"}], "*"], "\[LongEqual]", 
         StyleBox["A", "TI"], 
         SuperscriptBox[
          StyleBox["A", "TI"], "+"]}], TraditionalForm], "errors" -> {}, 
     "input" -> "(AA^+)^*=AA^+", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "a54286f8-24e1-4377-b053-58b1c6888c3a"],
 ", and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
            RowBox[{
              SuperscriptBox[
               StyleBox["A", "TI"], "+"], 
              StyleBox["A", "TI"]}], ")"}], "*"], "\[LongEqual]", 
         SuperscriptBox[
          StyleBox["A", "TI"], "+"], 
         StyleBox["A", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "(A^+A)^*=A^+A", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "499d54d1-fded-4d2b-816d-a2d4415ac730"],
 " (",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["A", "TI"], 
         SuperscriptBox[
          StyleBox["A", "TI"], "+"]}], TraditionalForm], "errors" -> {}, 
     "input" -> "AA^+", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "0b42f5a7-b9c5-432d-a505-e72774e7e8dd"],
 ", and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SuperscriptBox[
          StyleBox["A", "TI"], "+"], 
         StyleBox["A", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "A^+A", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "015abaa7-c85e-4566-9053-9a159ff1c6e9"],
 " are Hermitian)"
}], "Item",
 CellChangeTimes->{{3.9547225644921722`*^9, 3.9547225700956697`*^9}, {
  3.954723058848873*^9, 3.954723074043437*^9}, {3.954723611466197*^9, 
