    "]"}], "\n", 
   RowBox[{"MatrixPlot", "[", 
    RowBox[{"S1", ",", " ", 
     RowBox[{"PlotLabel", " ", "->", " ", "\"\<Shift Operator\>\""}]}], "]"}],
    "\n", 
   RowBox[{"MatrixPlot", "[", 
    RowBox[{"M1", ",", " ", 
     RowBox[{
     "PlotLabel", " ", "->", " ", "\"\<Multiplication Operator\>\""}]}], 
    "]"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"Calculate", " ", "operator", " ", "norms"}], " ", "*)"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<Operator norm of diagonal operator: \>\"", ",", " ", 
     RowBox[{"operatorNorm", "[", "D1", "]"}]}], "]"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<Operator norm of shift operator: \>\"", ",", " ", 
     RowBox[{"operatorNorm", "[", "S1", "]"}]}], "]"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<Operator norm of multiplication operator: \>\"", ",", " ", 
     RowBox[{"operatorNorm", "[", "M1", "]"}]}], "]"}]}]}]], "Input",
